@{
ViewBag.Title = "Opération Multirisque professionnel";
Layout = "~/Views/Shared/_LayoutMenuGestion.cshtml";
}



<!--begin::Content-->
<div class="content d-flex flex-column flex-column-fluid" id="kt_content">
    <!--begin::Subheader-->
    <div class="subheader py-2 py-lg-6 subheader-solid" id="kt_subheader" style="background: Beige">
        <div class="container-fluid d-flex align-items-center justify-content-between flex-wrap flex-sm-nowrap">
            <!--begin::Info-->
            <div class="d-flex align-items-center flex-wrap mr-1">
                <!--begin::Page Heading-->
                <div class="d-flex align-items-baseline flex-wrap mr-5">
                    <!--begin::Page Title-->
                    @*<h5 class="text-dark font-weight-bold my-1 mr-5">Modification d'écritures Comptables</h5>*@
                    <h5 class="text-dark font-weight-bold my-1 mr-5">GESTION</h5>&nbsp;&nbsp;
                    <!--end::Page Title-->
                    <!--begin::Breadcrumb-->
                    <ul class="breadcrumb breadcrumb-transparent breadcrumb-dot font-weight-bold p-0 my-2 font-size-sm">
                        <li class="breadcrumb-item">
                            <a href="" class="text-muted">Comptabilité</a>
                        </li>
                        <li class="breadcrumb-item">
                            <a href="" class="text-muted">Liste</a>
                        </li>
                        <li class="breadcrumb-item">
                            <a href="" class="text-muted">Modification d'écritures Comptables</a>
                        </li>
                    </ul>
                    <!--end::Breadcrumb-->
                </div>
                <!--end::Page Heading-->
            </div>
            <!--end::Info-->
            <!--begin::Toolbar-->
           
            <!--end::Toolbar-->
        </div>
    </div>
    <!--end::Subheader-->
    <!--begin::Entry-->
    <div class="d-flex flex-column-fluid">
        <!--begin::Container-->
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <!--begin::Card-->
                    <div class="card card-custom gutter-b example example-compact">
                        <div class="card-header" style="background-color: #F5F500">

                            <div class="card-title">
                                <span class="card-icon">
                                    <i class="fas fa-shopping-cart text-dark icon-xl"></i>
                                </span>
                                <h3 class="card-title" style="font-size: 30px; font-family: 'Maiandra GD'">Modification d'écritures Comptables</h3>
                            </div>
                        </div>
                        <!--begin::Form-->
                        <form class="form">
                            <div class="card-body">
                                <div class="row">
                                    <div class="col-12">
                                        <div class="mb-3">
                                            <div class="h4 text-dark-75">Liste des Ecritures :</div><br />
                                            @*<h3 class="font-size-lg text-dark-75 font-weight-bold mb-10">Liste des Ecritures </h3>*@
                                            <div class="mb-2">
                                                <div class="form-group row">
                                                    <div class="col-lg-3">
                                                        <label>* Date début:</label>
                                                        <div class="input-group">
                                                            <input type="text" class="form-control" name="url" placeholder="Entrer le numéro la date de début" />
                                                        </div>
                                                        <span class="form-text text-muted">Veuillez saisir la date de début.</span>
                                                    </div>
                                                    <div class="col-lg-3">
                                                        <label>*  Date fin:</label>
                                                        <div class="input-group">
                                                            <div class="input-group-prepend">
                                                                <span class="input-group-text">
                                                                    <i class="far fa-calendar-alt text-info"></i>
                                                                </span>
                                                            </div>
                                                            <input type="text" class="form-control" name="url" placeholder="Entrer la date de fin" />
                                                        </div>
                                                        <span class="form-text text-muted">Veuillez saisir la date de fin.</span>
                                                    </div>
                                                    <div class="col-lg-3">
                                                        <label>* N° Piece:</label>
                                                        <div class="input-group">
                                                            <div class="input-group-prepend">
                                                                <span class="input-group-text">
                                                                    <i class="fab fa-orcid text-info"></i>
                                                                </span>
                                                            </div>
                                                            <input type="text" class="form-control" name="url" placeholder="Entrer le N° Piece" />
                                                        </div>
                                                        <span class="form-text text-muted">Veuillez saisir le N° Piece.</span>
                                                    </div>
                                                    <div class="col-lg-3">
                                                        <label>Opérateur*</label>
                                                        <div class="input-group">
                                                            <div class="input-group-prepend">
                                                                <span class="input-group-text">
                                                                    <i class="fas fa-id-badge text-info"></i>
                                                                </span>
                                                            </div>
                                                            <select class="form-control" name="option">
                                                                <option value="">Sélectionner un opérateur</option>
                                                                <option>opérateur 1</option>
                                                                <option>opérateur 2</option>
                                                                <option>opérateur 3</option>
                                                                <option>opérateur 4</option>
                                                                <option>opérateur 5</option>
                                                            </select>
                                                        </div>
                                                        <span class="form-text text-muted">Veuillez sélectionnez un Operateur.</span>
                                                    </div>


                                                </div>
                                            </div>
                                            <div class="col-lg-3" style="margin-top:45px">
                                                <button type="submit" class="btn btn-primary btn-shadow-hover font-weight-bold">
                                                    <span>
                                                        <i class="far fa-check-circle"></i>
                                                        <span>Enregistrer</span>
                                                    </span>
                                                </button>
                                                <button class="btn btn-outline-warning" id="kt_reset">
                                                    <span>
                                                        <i class="la la-close"></i>
                                                        <span>Annuler</span>
                                                    </span>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="">
                                <div class="card card-custom mb-3">
                                    <div class="card-body">
                                        <!--begin: Datatable-->
                                        <div class="row">
                                            <div class="col-lg-12">
                                                <table class="table table-bordered table-checkable" id="kt_datatable">
                                                    <thead style="text-align:center">

                                                        <tr>
                                                            <th>DATE PIECE</th>
                                                            <th>CPT GENERAL</th>
                                                            <th>CPT TIERS </th>
                                                            <th>REFERENCE PIECE</th>
                                                            <th>LIBELLE OPERATION</th>
                                                            <th>DEBIT</th>
                                                            <th>CREDIT</th>
                                                        </tr>
                                                    </thead>
                                                    <tbody>
                                                        <tr>
                                                            <td>   </td>
                                                            <td>   </td>
                                                            <td>   </td>
                                                            <td>   </td>
                                                            <td>   </td>
                                                            <td>   </td>
                                                            <td>   </td>

                                                        </tr>

                                                    </tbody>
                                                </table>
                                            </div>



                                        </div>
                                    </div>

                                    <!--end: Datatable-->
                                </div>

                            </div>
                            <div class="separator separator-dashed my-10"></div>
                            <div class="card-body">
                                <div class="row">
                                    <div class="col-12">
                                        <div class="mb-3">
                                            @*<h3 class="font-size-lg text-dark-75 font-weight-bold mb-10"> Recherche Piece Comptable a Modifier :</h3>*@
                                            <div class="h4 text-dark-75"><strong> Recherche Piece Comptable a Modifier :</strong></div><br />
                                            <div class="mb-2">
                                                <div class="form-group row">
                                                    <div class="col-lg-6">
                                                        <label>* Date piece:</label>
                                                        <div class="input-group">
                                                            <div class="input-group-prepend">
                                                                <span class="input-group-text">
                                                                    <i class="far fa-calendar-alt text-info"></i>
                                                                </span>
                                                            </div>
                                                            <input type="text" class="form-control" name="url" placeholder="Entrer la date de la piece" />
                                                        </div>
                                                        <span class="form-text text-muted">Veuillez saisir la date de la piece.</span>
                                                    </div>

                                                    <div class="col-lg-6">
                                                        <label>* N° piece:</label>
                                                        <div class="input-group">
                                                            <div class="input-group-prepend">
                                                                <span class="input-group-text">
                                                                    <i class="fab fa-orcid text-info"></i>
                                                                </span>
                                                            </div>
                                                            <input type="text" class="form-control" name="url" placeholder="Entrer le numéro de la piece" />
                                                        </div>
                                                        <span class="form-text text-muted">Veuillez saisir le numéro de la piece.</span>
                                                    </div>

                                                </div>
                                            </div>

                                        </div>
                                        <div class="col-lg-6" style="margin-top:45px">
                                            <button class="btn btn-primary btn-primary--icon" id="kt_search">
                                                <span>
                                                    <i class="la la-search"></i>
                                                    <span>Rechercher</span>
                                                </span>
                                            </button>
                                            <button class="btn btn-outline-warning" id="kt_reset">
                                                <span>
                                                    <i class="la la-close"></i>
                                                    <span>Annuler</span>
                                                </span>
                                            </button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="separator separator-dashed my-10"></div>
                            <div class="card-body">
                                <div class="row">
                                    <div class="col-12">
                                        <div class="mb-3">
                                            @*<h3 class="font-size-lg text-dark-75 font-weight-bold mb-10"> OPERATION :</h3>*@
                                            <div class="h4 text-dark-75"><strong> Operation :</strong></div><br />
                                            <div class="">
                                                <div class="card card-custom mb-3">
                                                    <div class="card-body">
                                                        <!--begin: Datatable-->
                                                        <div class="row">
                                                            <div class="col-lg-12">
                                                                <table class="table table-bordered table-checkable" id="kt_datatable">
                                                                    <thead style="text-align:center">

                                                                        <tr>
                                                                            <th>DATE PIECE</th>
                                                                            <th>CPT GENERAL</th>
                                                                            <th>CPT TIERS </th>
                                                                            <th>REFERENCE PIECE</th>
                                                                            <th>LIBELLE OPERATION</th>
                                                                            <th>DEBIT</th>
                                                                            <th>CREDIT</th>
                                                                        </tr>
                                                                    </thead>
                                                                    <tbody>
                                                                        <tr>
                                                                            <td>   </td>
                                                                            <td>   </td>
                                                                            <td>   </td>
                                                                            <td>   </td>
                                                                            <td>   </td>
                                                                            <td>   </td>
                                                                            <td>   </td>

                                                                        </tr>
                                                                        <tr>
                                                                            <td colspan="5"></td>
                                                                            <td>0</td>
                                                                            <td>0</td>
                                                                        </tr>
                                                                    </tbody>
                                                                </table>
                                                            </div>



                                                        </div>
                                                    </div>

                                                    <!--end: Datatable-->
                                                </div>

                                            </div>

                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!--begin::Code example example-highlight  -->


                            <div class="card-footer">
                                <button type="submit" class="btn btn-primary btn-shadow-hover font-weight-bold">
                                    <span>
                                        <i class="far fa-check-circle"></i>
                                        <span>Enregistrer</span>
                                    </span>
                                </button>
                                <button class="btn btn-outline-warning" id="kt_reset">
                                    <span>
                                        <i class="la la-close"></i>
                                        <span>Annuler</span>
                                    </span>
                                </button>
                            </div>
                        </form>
                        <!--end::Form-->
                    </div>
                    <!--end::Card-->
                </div>
            </div>
        </div>
        <!--end::Container-->
    </div>
    <!--end::Entry-->
</div>
<!--end::Content-->
