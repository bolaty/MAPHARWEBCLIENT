@{
    ViewBag.Title = "Enregistrement Taxe";
    Layout = "~/Views/Shared/_LayoutMenuGestion.cshtml";
}


<!--begin::Content-->
<div class="content d-flex flex-column flex-column-fluid" id="kt_content">
    <!--begin::Subheader-->
    <div class="subheader py-2 py-lg-6 subheader-solid" id="kt_subheader" style="background-color: beige">
        <div class="container-fluid d-flex align-items-center justify-content-between flex-wrap flex-sm-nowrap">
            <!--begin::Info-->
            <div class="d-flex align-items-center flex-wrap mr-1">
                <!--begin::Page Heading-->
                <div class="d-flex align-items-baseline flex-wrap mr-5">
                    <!--begin::Page Title-->
                    @*<h5 class="text-dark font-weight-bold my-1 mr-5">Cloture d'exercice</h5>*@
                    <h3 class="text-dark font-weight-bold my-1 mr-5">GESTION</h3>

                    <!--end::Page Title-->
                    <!--begin::Breadcrumb-->
                    <ul class="breadcrumb breadcrumb-transparent breadcrumb-dot font-weight-bold p-0 my-2 font-size-sm">
                        <li class="breadcrumb-item">
                            <a href="" class="text-muted">ParamètreII</a>
                        </li>
                        <li class="breadcrumb-item">
                            <a href="" class="text-muted"> Enregistrement</a>
                        </li>
                        <li class="breadcrumb-item">
                            <a href="" class="text-muted">Taxe</a>
                        </li>
                    </ul>
                    <!--end::Breadcrumb-->
                </div>
                <!--end::Page Heading-->
            </div>
            <!--end::Info-->
            <!--begin::Toolbar-->
            <!--end::Toolbar-->
        </div>
    </div>
    <!--end::Subheader-->
    <!--begin::Entry-->
    <div class="d-flex flex-column-fluid">
        <!--begin::Container-->
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <!--begin::Card-->
                    <div class="card card-custom gutter-b example example-compact">
                        <div class="card-header" style="background-color: #F5F500;">
                            <div class="card-title">
                                <span class="card-icon">
                                    <i class="fas fa-percent text-dark icon-xl"></i>
                                </span>
                                <h3 class="card-title" style="font-size: 30px; font-family: 'Maiandra GD'">Taxe</h3>
                            </div>                            
                        </div>
                        <!--begin::Form-->
                        <form class="form">
                            <div class="card-body">
                                <div class="row">
                                    <div class="col-12">
                                        <div class="mb-3">
                                            <div class="row">
                                                <div class="col-lg-6">
                                                    <label>Code taxe*</label>
                                                    <div class="input-group">
                                                        <div class="input-group-prepend">
                                                            <span class="input-group-text">
                                                                <i class="fas fa-key text-info"></i>
                                                            </span>
                                                        </div>
                                                        <input type="text" class="form-control" name="url" placeholder="Entrer un code" />
                                                    </div>
                                                    <span class="form-text text-muted">Veuillez saisir le code.</span>
                                                </div>
                                                <div class="col-lg-6">
                                                    <label>Sens*</label>
                                                    <div class="input-group">
                                                        <div class="input-group-prepend">
                                                            <span class="input-group-text">
                                                                <i class="fas fa-exchange-alt text-info"></i>
                                                            </span>
                                                        </div>
                                                        <select class="form-control" name="option">
                                                            <option value="">Sélectionner un sens</option>
                                                            <option>sens 1</option>
                                                            <option>sens 2</option>
                                                            <option>sens 3</option>
                                                            <option>sens 4</option>
                                                            <option>sens 5</option>
                                                        </select>
                                                    </div>
                                                    <span class="form-text text-muted">Veuillez sélectionner le sens.</span>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-lg-12">
                                                    <label>Intitulé*</label>
                                                    <div class="input-group">
                                                        <div class="input-group-prepend">
                                                            <span class="input-group-text">
                                                                <i class="fas fa-file-alt text-info"></i>
                                                            </span>
                                                        </div>
                                                        <input type="text" class="form-control" name="url" placeholder="Entrer une intitulé" />
                                                    </div>
                                                    <span class="form-text text-muted">Veuillez saisir la intitulé.</span>
                                                </div>
                                            </div>
                                                <div class="row">
                                                    <div class="col-lg-6">
                                                        <label>Compte taxe*</label>
                                                        <div class="input-group">
                                                            <div class="input-group-prepend">
                                                                <span class="input-group-text">
                                                                    <i class="fas fa-fire text-info"></i>
                                                                </span>
                                                            </div>
                                                            <select class="form-control" name="option">
                                                                <option value="">Sélectionner un compte taxe</option>
                                                                <option>sens 1</option>
                                                                <option>sens 2</option>
                                                                <option>sens 3</option>
                                                                <option>sens 4</option>
                                                                <option>sens 5</option>
                                                            </select>
                                                        </div>
                                                        <span class="form-text text-muted">Veuillez sélectionner le compte taxe.</span>
                                                    </div>
                                                    <div class="col-lg-6">
                                                        <label>Taux*</label>
                                                        <div class="input-group">
                                                            <div class="input-group-prepend">
                                                                <span class="input-group-text">
                                                                    <i class="fas fa-percentage text-info"></i>
                                                                </span>
                                                            </div>
                                                            <select class="form-control" name="option">
                                                                <option value="">Sélectionner un taux</option>
                                                                <option>taux 1</option>
                                                                <option>taux 2</option>
                                                                <option>taux 3</option>
                                                                <option>taux 4</option>
                                                                <option>taux 5</option>
                                                            </select>
                                                        </div>
                                                        <span class="form-text text-muted">Veuillez sélectionner le taux.</span>
                                                    </div>
                                                    <div class="col-lg-12">
                                                        <label>Taxe*</label>
                                                        <div class="input-group">
                                                            <div class="input-group-prepend">
                                                                <span class="input-group-text">
                                                                    <i class="fas fa-percent text-info"></i>
                                                                </span>
                                                            </div>
                                                            <input type="text" class="form-control" name="url" placeholder="Entrer une taxe" />
                                                        </div>
                                                        <span class="form-text text-muted">Veuillez saisir la taxe.</span>
                                                    </div>
                                                </div>
                                            </div>
                                    </div>
                                </div>
                                <div class="row mt-8">
                                    <div class="col-lg-12">
                                        <button type="submit" class="btn btn-primary btn-shadow-hover font-weight-bold">
                                            <span>
                                                <i class="far fa-check-circle"></i>
                                                <span>Enregistrer</span>
                                            </span>
                                        </button>
                                        <button type="submit" class="btn btn-light-success font-weight-bold">
                                            <span>
                                                <i class="fas fa-undo"></i>
                                                <span>Réinitialiser</span>
                                            </span>
                                        </button>
                                        <a href="Taxe" class="btn btn-icon btn-light-dark mr-2" data-toggle="tooltip" data-theme="dark" title="Retour" style="position: absolute; right: 27px">
                                            <i class="fas fa-arrow-left"></i>
                                        </a>
                                    </div>
                                </div>
                            </div>
                            
                        </form>
                        <!--end::Form-->
                        <!--end::Card-->
                    </div>
                </div>
            </div>
        </div>
        <!--end::Container-->
    </div>
    <!--end::Entry-->
</div>
<!--end::Content-->
