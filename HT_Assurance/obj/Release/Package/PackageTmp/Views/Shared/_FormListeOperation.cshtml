<form class="mb-15" id="idList" novalidate name="nameList">
    <div class="card-body">
        <!--begin: Search Form-->
        <div class="row mb-6">
            <div class="col-lg-3 mb-lg-0 mb-6">
                <label>Période*</label>
                <div class="input-daterange input-group" id="">
                    <!--kt_datepicker-->
                    <input id="idPeriodeDu" type="text" class="form-control" ng-class="{'is-invalid':nameList.list_PeriodeDu.$invalid}" ng-pattern="/^([0][1-9]|[1|2][0-9]|[3][0|1])[./-]([0][1-9]|[1][0-2])[./-]([0-9]{4}|[0-9]{2})$/" name="list_PeriodeDu" placeholder="Du" ng-model="FormList.periodeDu" required maxlength="10" />
                    <div class="input-group-append">
                        <span class="input-group-text">
                            <i class="fas fa-arrows-alt-h text-info"></i>
                        </span>
                    </div>
                    <input id="idPeriodeAu" type="text" class="form-control" ng-class="{'is-invalid':nameList.list_PeriodeAu.$invalid}" ng-pattern="/^([0][1-9]|[1|2][0-9]|[3][0|1])[./-]([0][1-9]|[1][0-2])[./-]([0-9]{4}|[0-9]{2})$/" name="list_PeriodeAu" placeholder="Au" ng-model="FormList.periodeAu" required maxlength="10" />
                </div>
            </div>
            <div class="col-lg-3 mb-lg-0 mb-6">
                <label>N° police*</label>
                <div class="input-group">
                    <div class="input-group-prepend">
                        <span class="input-group-text">
                            <i class="fab fa-orcid text-info"></i>
                        </span>
                    </div>
                    <input type="text" class="form-control" ng-class="{'is-invalid':nameList.list_NumPolice.$invalid}" ng-pattern="/^[0-9]/" name="list_NumPolice" placeholder="Entrer un numéro" ng-model="FormList.numPolice" />
                    <div class="invalid-feeback text-danger" ng-show="nameList.$dirty && nameList.list_NumPolice.$invalid" style="font-size: 12px">Veuillez saisir un numéro correct.</div>
                </div>
            </div>
            <div class="col-lg-3 mb-lg-0 mb-6">
                <label>N° Souscripteur*</label>
                <div class="input-group">
                    <div class="input-group-prepend">
                        <span class="input-group-text">
                            <i class="fab fa-orcid text-info"></i>
                        </span>
                    </div>
                    <input type="text" class="form-control" ng-class="{'is-invalid':nameList.list_NumSouscripteur.$invalid}" ng-pattern="/^[0-9]/" name="list_NumSouscripteur" placeholder="Entrer un n° souscripteur" ng-model="FormList.numSouscripteur" />
                    <div class="invalid-feeback text-danger" ng-show="nameList.$dirty && nameList.list_NumSouscripteur.$invalid" style="font-size: 12px">Veuillez saisir un numéro correct.</div>
                </div>
            </div>
            <div class="col-lg-3 mb-lg-0 mb-6">
                <label>Nom du souscripteur*</label>
                <div class="input-group">
                    <div class="input-group-prepend">
                        <span class="input-group-text">
                            <i class="fas fa-address-card text-info"></i>
                        </span>
                    </div>
                    <input type="text" class="form-control" ng-class="{'is-invalid':nameList.list_NomSouscripteur.$invalid}" ng-pattern="/^[a-z-A-Z]/" name="list_NomSouscripteur" placeholder="Entrer un nom" ng-model="FormList.nomSouscripteur" />
                </div>
                <div class="invalid-feeback text-danger" ng-show="nameList.$dirty && nameList.list_NomSouscripteur.$invalid" style="font-size: 12px">Veuillez saisir un nom correct.</div>
            </div>
        </div>
        <div class="row mb-8">
            <div class="col-lg-6">
                <label>N° Commercial*</label>
                <div class="input-group">
                    <div class="input-group-prepend">
                        <span class="input-group-text">
                            <i class="fab fa-orcid text-info"></i>
                        </span>
                    </div>
                    <input type="text" class="form-control" ng-class="{'is-invalid':nameList.list_NumCommercial.$invalid}" ng-pattern="/^[0-9]/" name="list_NumCommercial" placeholder="Entrer un n° commercial" ng-model="FormList.numCommercial" />
                </div>
                <div class="invalid-feeback text-danger" ng-show="nameList.$dirty && nameList.list_NumCommercial.$invalid" style="font-size: 12px">Veuillez saisir un numéro correct.</div>
            </div>
            <div class="col-lg-6">
                <label>Nom du Commercial*</label>
                <div class="input-group">
                    <div class="input-group-prepend">
                        <span class="input-group-text">
                            <i class="fas fa-address-card text-info"></i>
                        </span>
                    </div>
                    <input type="text" class="form-control" ng-class="{'is-invalid':nameList.list_NomCommercial.$invalid}" ng-pattern="/^[a-z-A-Z]/" name="list_NomCommercial" placeholder="Entrer un nom" ng-model="FormList.nomCommercial" />
                </div>
                <div class="invalid-feeback text-danger" ng-show="nameList.$dirty && nameList.list_NomCommercial.$invalid" style="font-size: 12px">Veuillez saisir un nom correct.</div>
            </div>
        </div>

        <div class="row mt-8">
            <div class="col-lg-12">
                <button class="btn btn-primary btn-primary--icon" id="kt_search" ng-click="afficheListeOperation(nameList.$valid, FormList.periodeDu, FormList.periodeAu, FormList.numPolice, FormList.numSouscripteur, FormList.nomSouscripteur, FormList.numCommercial, FormList.nomCommercial)">
                    <span>
                        <i class="la la-search"></i>
                        <span>Rechercher</span>
                    </span>
                </button>&#160;&#160;
                <button type="button" class="btn btn-outline-warning" id="kt_reset" ng-click="listeAnnuler()">
                    <span>
                        <i class="la la-close"></i>
                        <span>Annuler</span>
                    </span>
                </button>
            </div>
        </div>&#160;&#160;
        <!--begin: Datatable-->
        <table class="table table-bordered table-hover table-checkable" id="kt_datatable">
            <thead style="text-align: center;">
                <!--background-color: #8CB9BA; color: White-->
                <tr>
                    <th>DATE</th>
                    <th>N° POLICE/N.C</th>
                    <th>SOUSCRIPTEUR</th>
                    <th>EFFET</th>
                    <th>ECHEANCE</th>
                    <th>ACTIONS</th>
                </tr>
            </thead>
            <tfoot ng-show="listeOperation[0].clsObjetRetour.SL_RESULTAT !== 'FALSE'">
                <tr ng-repeat="laListeOperation in listeOperation">
                    <td>{{laListeOperation.CA_DATESAISIE}}</td>
                    <td>{{laListeOperation.CA_NUMPOLICE}}</td>
                    <td>{{laListeOperation.TI_DENOMINATION}}</td>
                    <td>{{laListeOperation.CA_DATEEFFET}}</td>
                    <td>{{laListeOperation.CA_DATEECHEANCE}}</td>
                    <!--ligne des actions-->
                    <td>
                        <div class="example-preview" style="position: relative; right: -19px">
                            <div class="btn-toolbar" role="toolbar" aria-label="...">
                                <div class="btn-group mr-2" role="group" aria-label="...">
                                    <!--bouton voir plus-->
                                    <button type="button" class="btn btn-outline-secondary btn-icon" data-toggle="modal" data-target="#modalVoirPlus" ng-click="voirPlus(laListeOperation)">
                                        <i class="far fa-eye" data-toggle="tooltip" data-theme="dark" title="Voir plus"></i>
                                    </button>
                                    <!--bouton autres options-->
                                    <button type="button" class="btn btn-outline-secondary btn-icon" data-toggle="modal" data-target="#modalOption" ng-click="autreOption(laListeOperation.CA_CODECONTRAT)">
                                        <i class="fas fa-ellipsis-v" data-toggle="tooltip" data-theme="dark" title="Autres options"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </td>
                </tr>
            </tfoot>
        </table>
        <!--end: Datatable-->
    </div>
</form>
