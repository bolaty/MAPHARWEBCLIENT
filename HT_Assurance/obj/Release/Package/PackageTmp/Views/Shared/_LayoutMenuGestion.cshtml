                                        <span style="color: black; font-family: Maiandra GD; font-size: 20px">Tableau de bord</span>
                                    </span>
                                </a>
                            </li> *@

                            <!-- MENU GESTION -->
                            @Html.Partial("template_partials/_MenuGestion")

                            <!-- MENU ASSURANCE -->
                            @Html.Partial("template_partials/_MenuAssurances")

                            <!-- MENU GRH -->
                            @Html.Partial("template_partials/_MenuGrh")

                        </ul>
                        <!--end::Menu Nav-->
                    </div>
                    <!--end::Conteneur de menu-->
                </div>
                <!--end::Menu de coté-->
            </div>
            <!--end::De côté-->
            <!--begin::Wrapper-->
            <div class="d-flex flex-column flex-row-fluid wrapper" id="kt_wrapper">
                <!--begin::Header-->
                @Html.Partial("template_partials/_MenuHeader")
                <!--end::Header-->

                @RenderBody()
                <!--begin::Footer-->
                @Html.Partial("template_partials/_Footer")
                <!--end::Footer-->
            </div>
            <!--end::Wrapper-->
        </div>
        <!--end::Page-->
    </div>
