    <div class="modal fade" id="modalChangerDate" data-backdrop="static" tabindex="-1" role="dialog" aria-labelledby="modalChangerDate" aria-hidden="true" ng-controller="NewDateJourTravailController">
        <div class="modal-dialog modal-xs modal-dialog-centered" role="document">
            <div class="modal-content">
                <div class="modal-header" style="background-color: #B666D2;">
                    <h5 class="modal-title text-white" id="modalChangerDate" style="font-size: 23px; font-family: Maiandra GD">
                        <i class="fas fa-pen text-white"></i>
                        Saisie date de la journée de travail
                    </h5>
                </div>
                <div class="modal-body" style="text-align: center; font-size: 17px;">
                    <div class="col-12 text-right">
                    </div>
                    <div class="form-group col-lg-12">
                        <label style="position: relative; left: -170px">Nouvelle date</label>
                        <input id="idChampNewDate" type="text" class="form-control" ng-pattern="/^([0][1-9]|[1|2][0-9]|[3][0|1])[./-]([0][1-9]|[1][0-2])[./-]([0-9]{4}|[0-9]{2})$/" name="ChampNewDate" placeholder="Entrer la nouvelle date" ng-model="FormAddOperationTresorerie.journeeTravail" maxlength="10" />
                    </div>
                </div>
                <div class="modal-footer" style="background-color: beige">
                    <button class="btn btn-success btn-shadow-hover font-weight-bold" id="pourChoixDoc" ng-click="changedate(FormAddOperationTresorerie.journeeTravail)" data-dismiss="modal">
                        <span>
                            <i class="far fa-check-circle"></i>
                            <span>Enregistrer</span>
                        </span>
                    </button>
                    <button type="button" class="btn btn-outline-info" data-dismiss="modal">
                        <span>
                            <i class="la la-close"></i>
                            <span>Annuler</span>
                        </span>
                    </button>
                </div>
            </div>
        </div>
    </div>
    <!--end modal-->