    <!--begin modal pour transmission-->
    <div class="modal fade" id="modalTransmission" data-backdrop="static" tabindex="-1" role="dialog" aria-labelledby="modalTransmission" aria-hidden="true">
        <div class="modal-dialog modal-xs modal-dialog-top" role="document">
            <div class="modal-content">
                <div class="modal-header" style="background-color: #718E3E;">
                    <h5 class="modal-title text-white" id="modalTransmission" style="font-size: 23px; font-family: Maiandra GD">
                        <i class="fas fa-paper-plane text-white"></i>
                        Contrat
                    </h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <i aria-hidden="true" class="ki ki-close text-white"></i>
                    </button>
                </div>
                <div class="modal-body" style="text-align: center; font-size: 17px;">
                    Etes-vous sûr de vouloir transmettre ce contrat ?
                </div>
                <div class="modal-footer">
                    <button class="btn btn-dark btn-shadow font-weight-bold" id="pourTransmettre" ng-click="transmissionMP(idElement)" data-dismiss="modal">
                        <span>
                            <i class="fas fa-paper-plane"></i>
                            <span>Transmettre</span>
                        </span>
                    </button>
                    <button type="button" class="btn btn-outline-info" data-dismiss="modal">
                        <span>
                            <i class="la la-close"></i>
                            <span>Annuler</span>
                        </span>
                    </button>
                </div>
            </div>
        </div>
    </div>
    <!--end modal-->

