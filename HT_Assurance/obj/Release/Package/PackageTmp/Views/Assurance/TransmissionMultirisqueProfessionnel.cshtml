    <!--begin modal pour voir plus-->
    <div class="modal fade" id="modalVoirPlus" tabindex="-1" role="dialog" aria-labelledby="modalVoirPlus" aria-hidden="true">
        <div class="modal-dialog modal-dialog-scrollable modal-lg modal-dialog-centered" role="document">
            <div class="modal-content">
                <div class="modal-header" style="background-color: #718E3E;">
                    <h5 class="modal-title text-white" id="modalVoirPlus" style="font-size: 23px; font-family: Maiandra GD">
                        <i class="far fa-eye text-white"></i>
                        Plus d'informations
                    </h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <i aria-hidden="true" class="ki ki-close text-white"></i>
                    </button>
                </div>
                <div class="modal-body" style="height: 300px;">
                    <div class="row col-lg-12">
                        <div class="col-lg-6">
                            <h6><strong style="color: DarkSlateBlue">INFORMATION SOUSCRIPTEUR</strong></h6>
                            <strong style="font-size: 13px; font-family: 'Maiandra GD'">SOUSCRIPTEUR</strong> : {{recuperer.TI_DENOMINATION}} <br>
                        </div>
                        <div class="col-lg-6">
                            <h6><strong style="color: DarkSlateBlue">INFORMATION COMMERCIAL</strong></h6>
                            <strong style="font-size: 13px; font-family: 'Maiandra GD'">NOM DU COMMERCIAL</strong> : {{recuperer.TI_DENOMINATIONCOMMERCIAL}} <br>
                        </div>
                    </div>&#160;&#160;
                    <div class="row col-lg-12">
                        <div class="col-lg-6">
                            <h6><strong style="color: DarkSlateBlue">INFORMATION GENERALE</strong></h6>
                            <strong style="font-size: 13px; font-family: 'Maiandra GD'">DATE</strong> : {{recuperer.CA_DATESAISIE}} <br>
                            <strong style="font-size: 13px; font-family: 'Maiandra GD'">N° POLICE/N.C</strong> : {{recuperer.CA_NUMPOLICE}} <br>
                            <strong style="font-size: 13px; font-family: 'Maiandra GD'">INTERMEDIAIRE</strong> : {{recuperer.IT_DENOMMINATION}} <br>
                            <strong style="font-size: 13px; font-family: 'Maiandra GD'">PAYS</strong> : {{recuperer.PY_LIBELLE}} <br>
                            <strong style="font-size: 13px; font-family: 'Maiandra GD'">VILLE</strong> : {{recuperer.VL_LIBELLE}} <br>
                            <strong style="font-size: 13px; font-family: 'Maiandra GD'">COMMUNE</strong> : {{recuperer.CO_LIBELLE}} <br>
                            <strong style="font-size: 13px; font-family: 'Maiandra GD'">SITUATION GEO.</strong> : {{recuperer.CA_SITUATIONGEOGRAPHIQUE}} <br>
                        </div>
                        <div class="col-lg-6">
                            <strong style="font-size: 13px; font-family: 'Maiandra GD'">EFFET</strong> : {{recuperer.CA_DATEEFFET}} <br>
                            <strong style="font-size: 13px; font-family: 'Maiandra GD'">ECHEANCE</strong> : {{recuperer.CA_DATEECHEANCE}} <br>
                            <strong style="font-size: 13px; font-family: 'Maiandra GD'">ASSUREUR</strong> : {{recuperer.TI_DENOMINATIONASSUREUR}} <br>
                            <strong style="font-size: 13px; font-family: 'Maiandra GD'">INTERLOCUTEUR</strong> : {{recuperer.CA_NOMINTERLOCUTEUR}} <br>
                            <strong style="font-size: 13px; font-family: 'Maiandra GD'">TELEPHONE INTERLOCUTEUR</strong> : {{recuperer.CA_CONTACTINTERLOCUTEUR}} <br>
                            <strong style="font-size: 13px; font-family: 'Maiandra GD'">DESIGNATION</strong> : {{recuperer.DI_LIBELLEDESIGNATION}} <br>
                            <strong style="font-size: 13px; font-family: 'Maiandra GD'">TYPE D'AFFAIRE</strong> : {{recuperer.TA_LIBLLETYPEAFFAIRES}} <br>
                        </div>
