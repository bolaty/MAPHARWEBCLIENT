@{
    ViewBag.Title = "Opération Multirisque professionnel";
    Layout = "~/Views/Shared/_LayoutMenuGestion.cshtml";
}



<!--begin::Content-->
<div class="content d-flex flex-column flex-column-fluid" id="kt_content">
    <!--begin::Subheader-->
    <div class="subheader py-2 py-lg-6 subheader-solid" id="kt_subheader" style="background: Beige">
        <div class="container-fluid d-flex align-items-center justify-content-between flex-wrap flex-sm-nowrap">
            <!--begin::Info-->
            <div class="d-flex align-items-center flex-wrap mr-1">
                <!--begin::Page Heading-->
                <div class="d-flex align-items-baseline flex-wrap mr-5">
                    <!--begin::Page Title-->
                    @*<h5 class="text-dark font-weight-bold my-1 mr-5">Extourne</h5>*@
                    <h5 class="text-dark font-weight-bold my-1 mr-5">GESTION</h5>&nbsp;&nbsp;
                    <!--end::Page Title-->
                    <!--begin::Breadcrumb-->
                    <ul class="breadcrumb breadcrumb-transparent breadcrumb-dot font-weight-bold p-0 my-2 font-size-sm">
                        <li class="breadcrumb-item">
                            <a href="" class="text-muted">Articles et Prestations</a>
                        </li>
                        <li class="breadcrumb-item">
                            <a href="" class="text-muted">Liste</a>
                        </li>
                        <li class="breadcrumb-item">
                            <a href="" class="text-muted">Configuration des Comptes</a>
                        </li>
                    </ul>
                    <!--end::Breadcrumb-->
                </div>
                <!--end::Page Heading-->
            </div>
            <!--end::Info-->
            <!--begin::Toolbar-->
            <!--end::Toolbar-->
        </div>
    </div>
    <!--end::Subheader-->
    <!--begin::Entry-->
    <div class="d-flex flex-column-fluid">
        <!--begin::Container-->
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <!--begin::Card-->
                    <div class="card card-custom gutter-b example example-compact">
                        <div class="card-header" style="background-color: #F5F500">

                            <div class="card-title">
                                <span class="card-icon">
                                    <i class="fas fa-shopping-cart text-dark icon-xl"></i>
                                </span>
                                <h3 class="card-title" style="font-size: 30px; font-family: 'Maiandra GD'">Configuration des Comptes</h3>
                            </div>
                        </div>
                        <!--begin::Form-->
                        <form class="form">
                            <div class="card-body">
                                <div class="row">
                                    <div class="col-12">
                                        <div class="mb-3">
                                            <div  class="form-group row">
                                                <div class="col-lg-6" style="margin-top:25px">
                                                    <label> Code*</label>
                                                    <div class="input-group">
                                                        <div class="input-group-prepend">
                                                            <span class="input-group-text">
                                                                <i class="fas fa-key text-info"></i>
                                                            </span>
                                                        </div>
                                                        <input type="text" class="form-control" name="url" placeholder="Charger Ecriture" />
                                                    </div>
                                                </div>
                                                <div class="col-lg-8" style="margin-top:25px">
                                                    <label>Libelle*</label>
                                                    <div class="input-group">
                                                        <div class="input-group-prepend">
                                                            <span class="input-group-text">
                                                                <i class="fas fa-address-card text-info"></i>
                                                            </span>
                                                        </div>
                                                        <input type="text" class="form-control" name="url" placeholder="entrez le nom du client" />
                                                    </div>
                                                </div>
                                                <div class="col-lg-5" style="margin-top:20px">
                                                    <label>Operation * :</label>
                                                    <div class="input-group">
                                                        <div class="input-group-prepend">
                                                            <span class="input-group-text">
                                                                <i class="fas fa-quote-left text-info"></i>
                                                            </span>
                                                        </div>
                                                        <select class="form-control" name="option">
                                                            <option value=""></option>
                                                            <option></option>
                                                            <option></option>
                                                            <option></option>
                                                            <option></option>
                                                            <option></option>
                                                        </select>
                                                    </div>
                                                    @*<span class="form-text text-muted">Veuillez sélectionnez un annee.</span>*@
                                                </div>
                                                <div class="col-lg-3" style="margin-top:20px">
                                                    <label>* N° Compte:</label>
                                                    <div class="input-group">
                                                        <div class="input-group-prepend">
                                                            <span class="input-group-text">
                                                                <i class="fab fa-orcid text-info"></i>
                                                            </span>
                                                        </div>
                                                        <input type="text" class="form-control" name="url" placeholder="Entrer le N° de Compte 1" />

                                                    </div>


                                                </div>
                                                <div class="col-lg-2 mb-lg-0 mb-6" style="position: relative; top: 40px">
                                                    <button class="btn btn-secondary btn-secondary--icon" id="kt_reset">
                                                        ...
                                                    </button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="card-body">
                                <div class="row">
                                    <div class="col-12">
                                        <div class="mb-3">
                                            <div class="">
                                                <div class="card card-custom mb-3">
                                                    <div class="card-body">
                                                        <!--begin: Datatable-->
                                                        <div class="row">
                                                            <div class="col-lg-12">
                                                                <table class="table table-bordered table-checkable" id="kt_datatable">
                                                                    <thead style="text-align: center;">
                                                                        <tr>
                                                                            <th>CODE</th>
                                                                            <th>LIBELLE</th>
                                                                        </tr>
                                                                    </thead>
                                                                    <tbody>
                                                                        <tr>
                                                                            <td>   </td>
                                                                            <td>   </td>
                                                                        </tr>
                                                                    </tbody>
                                                                </table>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <!--end: Datatable-->
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!--end::Form-->
                            <div class="card-footer">
                                <button type="submit" class="btn btn-primary btn-shadow-hover font-weight-bold">
                                    <span>
                                        <i class="far fa-check-circle"></i>
                                        <span>Enregistrer</span>
                                    </span>
                                </button>
                                <button class="btn btn-outline-warning" id="kt_reset">
                                    <span>
                                        <i class="la la-close"></i>
                                        <span>Annuler</span>
                                    </span>
                                </button>
                            </div>
                        </form>
                    </div>
                    <!--end::Card-->
                </div>
            </div>
        </div>
        <!--end::Container-->
    </div>
    <!--end::Entry-->
</div>
<!--end::Content-->
