            <!--begin::Info-->
            <div class="d-flex align-items-center flex-wrap mr-1">
                <!--begin::Page Heading-->
                <div class="d-flex align-items-baseline flex-wrap mr-5">
                    <!--begin::Page Title-->
                    @*<h5 class="text-dark font-weight-bold my-1 mr-5">Ecritures Comptables En Attente</h5>*@
                    <h5 class="text-dark font-weight-bold my-1 mr-5">GESTION</h5>&nbsp;&nbsp;
                    <!--end::Page Title-->
                    <!--begin::Breadcrumb-->
                    <ul class="breadcrumb breadcrumb-transparent breadcrumb-dot font-weight-bold p-0 my-2 font-size-sm">
                        <li class="breadcrumb-item">
                            <a href="" class="text-muted">Comptabilité</a>
                        </li>
                        <li class="breadcrumb-item">
                            <a href="" class="text-muted">liste</a>
                        </li>
                        <li class="breadcrumb-item">
                            <a href="" class="text-muted">Ecritures Comptables En Attente</a>
                        </li>
                    </ul>
                    <!--end::Breadcrumb-->
                </div>
                <!--end::Page Heading-->
            </div>
            <!--end::Info-->
            <!--begin::Toolbar-->
           
            <!--end::Toolbar-->
        </div>
    </div>
    <!--end::Subheader-->
    <!--begin::Entry-->
    <div class="d-flex flex-column-fluid">
        <!--begin::Container-->
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <!--begin::Card-->
                    <div class="card card-custom gutter-b example example-compact">
                        <div class="card-header" style="background-color: #F5F500">
                            <div class="card-title">
                                <span class="card-icon">
                                    <i class="fas fa-shopping-cart text-dark icon-xl"></i>
                                </span>
                                <h3 class="card-title" style="font-size: 30px; font-family: 'Maiandra GD'">Ecritures Comptables En Attente</h3>
                            </div>
                        </div>
                        <!--begin::Form-->
                        <form class="form">
                            <div class="card-body">
                                <div class="row">
                                    <div class="col-12">
                                        <div class="mb-3">
                                            <div class="h4 text-dark-75"><strong> Information :</strong></div><br />
                                            @*<h3 class="font-size-lg text-dark-75 font-weight-bold mb-10">INFORMATION </h3>*@
                                            <div class="mb-2">
                                                <div class="form-group row">
                                                    <div class="col-lg-6">
                                                        <label>* N° Bordereau:</label>
                                                        <div class="input-group">
                                                            <div class="input-group-prepend">
                                                                <span class="input-group-text">
                                                                    <i class="fab fa-orcid text-info"></i>
                                                                </span>
                                                            </div>
                                                            <input type="text" class="form-control" name="url" placeholder="Entrer le numéro du Bordereau" />
                                                        </div>
                                                        <span class="form-text text-muted">Veuillez saisir de numéro du Bordereau.</span>
                                                    </div>
                                                    <div class="col-lg-6">
                                                        <label>* Solde Compte:</label>
                                                        <div class="input-group">
                                                            <div class="input-group-prepend">
                                                                <span class="input-group-text">
                                                                    <i class="fas fa-money-bill text-info"></i>
                                                                </span>
                                                            </div>
                                                            <input type="text" class="form-control" name="url" placeholder="Entrer le  Solde Compte" />
                                                        </div>
                                                        <span class="form-text text-muted">Veuillez saisir le  Solde Compte.</span>
                                                    </div>
                                                    <div class="col-lg-6" style="margin-top:20px">
                                                        <label>* Journal:</label>
                                                        <div class="input-group">
                                                            <div class="input-group-prepend">
                                                                <span class="input-group-text">
                                                                    <i class="fab fa-readme text-info"></i>
                                                                </span>
                                                            </div>
                                                            <select class="form-control" name="option">
                                                                <option value="">Sélectionner le Journal</option>
                                                                <option>nature 1</option>
                                                                <option>nature 2</option>
                                                                <option>nature 3</option>
                                                                <option>nature 4</option>
                                                                <option>nature 5</option>
                                                            </select>
                                                        </div>
                                                        <span class="form-text text-muted">Veuillez sélectionnez un Journal.</span>
                                                    </div>
                                                    <div class="col-lg-6" style="margin-top:20px">
                                                        <label>* Solde Operateur:</label>
                                                        <div class="input-group">
                                                            <div class="input-group-prepend">
                                                                <span class="input-group-text">
                                                                    <i class="fas fa-money-bill text-info"></i>
                                                                </span>
                                                            </div>
                                                            <input type="text" class="form-control" name="url" placeholder="Entrer le  Solde Operateur" />
                                                        </div>
                                                        <span class="form-text text-muted">Veuillez saisir le Solde Operateur.</span>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="separator separator-dashed my-10"></div>
                            <div class="card-body">
                                <div class="row">
                                    <div class="col-12">
                                        <div class="mb-3">
                                            @*<h3 class="font-size-lg text-dark-75 font-weight-bold mb-10"> OPERATION :</h3>*@
                                            <div class="h4 text-dark-75"><strong> Operation :</strong></div><br />
                                            <div class="">
                                                <div class="card card-custom mb-3">
                                                    <div class="card-body">
                                                        <!--begin: Datatable-->
                                                        <div class="row">
                                                            <div class="col-lg-12">
                                                                <table class="table table-bordered table-checkable" id="kt_datatable">
                                                                    <thead>
                                                                        <tr>
                                                                            <td><input type="text" class="form-control" disabled /> </td>
                                                                            <td>
                                                                                <div class="input-group">
                                                                                    <input type="text" class="form-control" />
                                                                                    <button class="btn btn-secondary btn-secondary--icon" id="kt_reset">
                                                                                        ...
                                                                                    </button>
                                                                                </div>

                                                                            </td>
                                                                            <td>
                                                                                <div class="input-group">
                                                                                    <input type="text" class="form-control" />
                                                                                    <button class="btn btn-secondary btn-secondary--icon" id="kt_reset">
                                                                                        ...
                                                                                    </button>
                                                                                </div>

                                                                            </td>   
                                                                            <td> <input type="text" class="form-control" />  </td>
                                                                            <td> <input type="text" class="form-control" />  </td>
                                                                            <td> <input type="text" class="form-control" />  </td>
                                                                            <td> <input type="text" class="form-control" />  </td>

                                                                        </tr>
                                                                        <tr>
                                                                            <th>DATE PIECE</th>
                                                                            <th>CPT GENERAL</th>
                                                                            <th>CPT TIERS </th>
                                                                            <th>REFERENCE PIECE</th>
                                                                            <th>LIBELLE OPERATION</th>
                                                                            <th>DEBIT</th>
                                                                            <th>CREDIT</th>
                                                                        </tr>
                                                                    </thead>
                                                                    <tbody>
                                                                        <tr>
                                                                            <td>   </td>
                                                                            <td>   </td>
                                                                            <td>   </td>
                                                                            <td>   </td>
                                                                            <td>   </td>
                                                                            <td>   </td>
                                                                            <td>   </td>

                                                                        </tr>
                                                                        <tr>
                                                                            <td colspan="5"></td>
                                                                            <td>0</td>
                                                                            <td>0</td>
                                                                        </tr>
                                                                    </tbody>
                                                                </table>
                                                            </div>



                                                        </div>
                                                    </div>

                                                    <!--end: Datatable-->
                                                </div>

                                            </div>

                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!--begin::Code example example-highlight  -->


                            <div class="card-footer">
                                <button type="submit" class="btn btn-primary btn-shadow-hover font-weight-bold">
                                    <span>
                                        <i class="far fa-check-circle"></i>
                                        <span>Enregistrer</span>
                                    </span>
                                </button>
                                <button class="btn btn-outline-warning" id="kt_reset">
                                    <span>
                                        <i class="la la-close"></i>
                                        <span>Annuler</span>
                                    </span>
                                </button>
                            </div>
                        </form>
                        <!--end::Form-->
                    </div>
                    <!--end::Card-->
                </div>
            </div>
        </div>
        <!--end::Container-->
    </div>
    <!--end::Entry-->
</div>
<!--end::Content-->
