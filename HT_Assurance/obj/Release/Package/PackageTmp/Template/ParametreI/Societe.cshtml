@{
    ViewBag.Title = "Societe";
    Layout = "~/Views/Shared/_LayoutMenuGestion.cshtml";
}


<!--begin::Content-->
<div class="content d-flex flex-column flex-column-fluid" id="kt_content">
    <!--begin::Subheader-->
    <div class="subheader py-2 py-lg-6 subheader-solid" id="kt_subheader" style="background: Beige">
        <div class="container-fluid d-flex align-items-center justify-content-between flex-wrap flex-sm-nowrap">
            <!--begin::Info-->
            <div class="d-flex align-items-center flex-wrap mr-1">
                <!--begin::Page Heading-->
                <div class="d-flex align-items-baseline flex-wrap mr-5">
                    <!--begin::Page Title-->
                    <h5 class="text-dark font-weight-bold my-1 mr-5">GESTION</h5>
                    <!--end::Page Title-->
                    <!--begin::Breadcrumb-->
                    <ul class="breadcrumb breadcrumb-transparent breadcrumb-dot font-weight-bold p-0 my-2 font-size-sm">
                        <li class="breadcrumb-item">
                            <a href="" class="text-muted">Paramètre I</a>
                        </li>
                        <li class="breadcrumb-item">
                            <a href="" class="text-muted">Enregistrement</a>
                        </li>
                        <li class="breadcrumb-item">
                            <a href="" class="text-muted">Société</a>
                        </li>
                    </ul>
                    <!--end::Breadcrumb-->
                </div>
                <!--end::Page Heading-->
            </div>
            <!--end::Info-->
            <!--begin::Toolbar-->
            <!--end::Toolbar-->
        </div>
    </div>
    <!--end::Subheader-->
    <!--begin::Entry-->
    <div class="d-flex flex-column-fluid">
        <!--begin::Container-->
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <!--begin::Card-->
                    <div class="card card-custom gutter-b example example-compact">
                        <div class="card-header" style="background-color: #F5F500">
                            <div class="card-title">
                                <span class="card-icon">
                                    <i class="fas fa-users text-dark icon-xl"></i>
                                </span>
                                <h3 class="card-title" style="font-size: 30px; font-family: 'Maiandra GD'">Société</h3>
                            </div>                           
                        </div>
                        <!--begin::Form-->
                        <form class="form">
                            <div class="card-body">
                                <div class="row">
                                    <div class="col-12">
                                        <div class="mb-3">                                            
                                            <div class="form-group row">
                                                <div class="col-lg-4">
                                                    <label>Code*</label>
                                                    <div class="input-group">
                                                        <div class="input-group-prepend">
                                                            <span class="input-group-text">
                                                                <i class="fas fa-key text-info"></i>
                                                            </span>
                                                        </div>
                                                        <input type="text" class="form-control" name="url" placeholder="Entrer un code " />
                                                    </div>
                                                    <span class="form-text text-muted">Veuillez saisir le code.</span>
                                                </div>
                                                <div class="col-lg-4">
                                                    <label>Raison sociale*</label>
                                                    <div class="input-group">
                                                        <div class="input-group-prepend">
                                                            <span class="input-group-text">
                                                                <i class="fab fa-rust text-info"></i>
                                                            </span>
                                                        </div>
                                                        <input type="text" class="form-control" name="url" placeholder="Entrer une raison sociale" />
                                                    </div>
                                                    <span class="form-text text-muted">Veuillez saisir la raison sociale.</span>
                                                </div>
                                                <div class="col-lg-4">
                                                    <label>Téléphone*</label>
                                                    <div class="input-group">
                                                        <div class="input-group-prepend">
                                                            <span class="input-group-text">
                                                                <i class="fas fa-phone-alt text-info"></i>
                                                            </span>
                                                        </div>
                                                        <input type="text" class="form-control" name="url" placeholder="Entrer un téléphone" />
                                                    </div>
                                                    <span class="form-text text-muted">Veuillez saisir le téléphone.</span>
                                                </div>
                                                <div class="col-lg-4">
                                                    <label>Boite postale*</label>
                                                    <div class="input-group">
                                                        <div class="input-group-prepend">
                                                            <span class="input-group-text">
                                                                <i class="far fa-envelope text-info"></i>
                                                            </span>
                                                        </div>
                                                        <input type="text" class="form-control" name="url" placeholder="Entrer une boite postale" />
                                                    </div>
                                                    <span class="form-text text-muted">Veuillez saisir la boite postale.</span>
                                                </div>
                                                <div class="col-lg-4">
                                                    <label>Email*</label>
                                                    <div class="input-group">
                                                        <div class="input-group-prepend">
                                                            <span class="input-group-text">
                                                                <i class="fas fa-at text-info"></i>
                                                            </span>
                                                        </div>
                                                        <input type="text" class="form-control" name="url" placeholder="Entrer un email" />
                                                    </div>
                                                    <span class="form-text text-muted">Veuillez saisir l'email</span>
                                                </div>
                                                <div class="col-lg-4">
                                                    <label>Site web*</label>
                                                    <div class="input-group">
                                                        <div class="input-group-prepend">
                                                            <span class="input-group-text">
                                                                <i class="fas fa-globe text-info"></i>
                                                            </span>
                                                        </div>
                                                        <input type="text" class="form-control" name="url" placeholder="Entrer un site web" />
                                                    </div>
                                                    <span class="form-text text-muted">Veuillez saisir le site web.</span>
                                                </div>
                                                <div class="col-lg-4">
                                                    <label>Directeur*</label>
                                                    <div class="input-group">
                                                        <div class="input-group-prepend">
                                                            <span class="input-group-text">
                                                                <i class="fas fa-user-tie text-info"></i>
                                                            </span>
                                                        </div>
                                                        <input type="text" class="form-control" name="url" placeholder="Entrer un directeur" />
                                                    </div>
                                                    <span class="form-text text-muted">Veuillez saisir le directeur.</span>
                                                </div>
                                                <div class="col-lg-4">
                                                    <label>Slogan*</label>
                                                    <div class="input-group">
                                                        <div class="input-group-prepend">
                                                            <span class="input-group-text">
                                                                <i class="fas fa-tag text-info"></i>
                                                            </span>
                                                        </div>
                                                        <input type="text" class="form-control" name="url" placeholder="Entrer un slogan" />
                                                    </div>
                                                    <span class="form-text text-muted">Veuillez saisir le slogan.</span>
                                                </div>
                                            </div>                                                                                                                                                                                                                        
                                        </div>
                                    </div>
                                    <!--begin::Code example example-highlight  -->
                                    <!--end::Code example-->
                                </div>
                            </div>
                            <div class="card-footer">
                                <button type="submit" class="btn btn-primary btn-shadow-hover font-weight-bold">
                                    <span>
                                        <i class="far fa-check-circle"></i>
                                        <span>Enregistrer</span>
                                    </span>
                                </button>&#160;&#160;
                                <button class="btn btn-outline-warning" id="kt_reset">
                                    <span>
                                        <i class="la la-close"></i>
                                        <span>Annuler</span>
                                    </span>
                                </button>
                            </div>
                        </form>
                        <!--end::Form-->
                    </div>
                    <!--end::Card-->
                </div>
            </div>
        </div>
        <!--end::Container-->
    </div>
    <!--end::Entry-->
</div>
<!--end::Content-->
