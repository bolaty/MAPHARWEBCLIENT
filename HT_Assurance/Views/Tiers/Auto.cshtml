                <div class="card-body">
                    <!--begin: Search Form-->
                    <form class="mb-15">
                        <div class="row mb-6">
                            <div class="col-lg-4 mb-lg-0 mb-6">
                                <label>Période*</label>
                                <div class="input-daterange input-group" id="kt_datepicker">
                                    <input type="text" class="form-control datatable-input" name="start" placeholder="Du" data-col-index="5" />
                                    <div class="input-group-append">
                                        <span class="input-group-text">
                                            <i class="fas fa-arrows-alt-h text-info"></i>
                                        </span>
                                    </div>
                                    <input type="text" class="form-control datatable-input" name="end" placeholder="Au" data-col-index="5" />
                                </div>
                            </div>
                            <div class="col-lg-4 mb-lg-0 mb-6">
                                <label>N° police*</label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">
                                            <i class="fab fa-expeditedssl text-info"></i>
                                        </span>
                                    </div>
                                    <input type="text" class="form-control datatable-input" placeholder="Entrer le numéro de la police" data-col-index="4" />
                                </div>
                            </div>
                            <div class="col-lg-4 mb-lg-0 mb-6">
                                <label>N° Souscripteur*</label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">
                                            <i class="fas fa-key text-info"></i>
                                        </span>
                                    </div>
                                    <select class="form-control datatable-input" data-col-index="2">
                                        <option value="">Sélectionner n° souscripteur</option>
                                        <option value="">N° souscripteur 1</option>
                                        <option value="">N° souscripteur 2</option>
                                        <option value="">N° souscripteur 3</option>
                                        <option value="">N° souscripteur 4</option>
                                        <option value="">N° souscripteur 5</option>
                                    </select>
                                </div>
                            </div>

                        </div>
                        <div class="row mb-8">
                            <div class="col-lg-4 mb-lg-0 mb-6">
                                <label>Nom du souscripteur*</label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">
                                            <i class="fas fa-address-card text-info"></i>
                                        </span>
                                    </div>
                                    <input type="text" class="form-control datatable-input" placeholder="Entrer le nom du souscripteur" data-col-index="4" />
                                </div>
                            </div>
                            <div class="col-lg-4 mb-lg-0 mb-6">
                                <label>N° Commercial*</label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">
                                            <i class="fas fa-key text-info"></i>
                                        </span>
                                    </div>
                                    <input type="text" class="form-control datatable-input" placeholder="Entrer le numéro du commercial" data-col-index="1" />
                                </div>
                            </div>
                            <div class="col-lg-4 mb-lg-0 mb-6">
                                <label>Nom du Commercial*</label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">
                                            <i class="fas fa-address-card text-info"></i>
                                        </span>
                                    </div>
                                    <input type="text" class="form-control datatable-input" placeholder="Entrer le nom du commercial" data-col-index="0" />
                                </div>
                            </div>                 
                        </div>

                        <div class="row mt-8">
                            <div class="col-lg-12">
                                <button class="btn btn-primary btn-primary--icon" id="kt_search">
                                    <span>
                                        <i class="la la-search"></i>
                                        <span>Rechercher</span>
                                    </span>
                                </button>&#160;&#160;
                                <button class="btn btn-outline-warning" id="kt_reset">
                                    <span>
                                        <i class="la la-close"></i>
                                        <span>Annuler</span>
                                    </span>
                                </button>
                            </div>
                        </div>
                    </form>
