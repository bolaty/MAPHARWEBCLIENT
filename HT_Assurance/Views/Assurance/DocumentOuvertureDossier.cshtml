        </div>
        <!--end::Container-->
    </div>


    <!--end::Entry-->
    <div class="modal fade" id="modalVoirPlus" tabindex="-1" role="dialog" aria-labelledby="modalVoirPlus" aria-hidden="true">
        <div class="modal-dialog modal-dialog-scrollable modal-lg modal-dialog-centered" role="document">
            <div class="modal-content">
                <div class="modal-header" style="background-color: #718E3E;">
                    <h5 class="modal-title text-white" id="modalVoirPlus" style="font-size: 23px; font-family: Maiandra GD">
                        <i class="fas fa-book text-white icon-xl"></i>
                        Ajout de fichier
                    </h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <i aria-hidden="true" class="ki ki-close text-white"></i>
                    </button>
                </div>
                <div class="modal-body" style="height: 180px;">
                        <div class="text-center">
                            <form class="kt-form" id="kt_form_Doc" enctype="multipart/form-data">
                                <div class="kt-wizard-v1__form ">
                                    <div  class="row alert alert alert-outline-info" role="alert" style="text-align: left;">
                                        <div class="col-12 text-right"><!--<a href="" ng-click="deleteSTDocUpdate($index)" class="text-danger"><i class="flaticon2-trash"></i></a>--></div>
                                        <div class="form-group col-lg-6">
                                            <label for="">Télécharger le fichier</label>
                                            <input  type="file" class="form-control"  required>
                                        </div>
                                        <div class="form-group col-lg-6">
                                            <label for="">Nommer le fichier</label>
                                            <input type="text" name="nomFile" maxlength="150" class="form-control"  placeholder="Nom du fichier" required>
                                        </div>
                                    </div>
                               
                                </div>
                            </form>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-info" data-dismiss="modal">Fermer</button>
                        <button type="button" class="btn btn-success">Enregistrer</button>
                    </div>
                </div>
               
            </div>
        </div>
    </div>
    <!--end modal-->
    <!--begin modal pour suppression-->
    <div class="modal fade" id="modalSupprimer" data-backdrop="static" tabindex="-1" role="dialog" aria-labelledby="modalSupprimer" aria-hidden="true">
        <div class="modal-dialog modal-xs modal-dialog-top" role="document">
            <div class="modal-content">
                <div class="modal-header" style="background-color: #718E3E;">
                    <h5 class="modal-title text-white" id="modalSupprimer" style="font-size: 23px; font-family: Maiandra GD">
                        <i class="fas fa-ban text-white"></i>
                        Contrat
                    </h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <i aria-hidden="true" class="ki ki-close text-white"></i>
                    </button>
                </div>
                <div class="modal-body" style="text-align: center; font-size: 17px;">
                    Etes-vous sûr de vouloir supprimer ce contrat ?
                </div>
                <div class="modal-footer">
                    <button class="btn btn-danger btn-shadow font-weight-bold" id="kt_reset" ng-click="supprimerCheque()" data-dismiss="modal">
                        <span>
                            <i class="fas fa-ban"></i>
                            <span>Supprimer</span>
                        </span>
                    </button>
                    <button type="button" class="btn btn-outline-info" data-dismiss="modal">
                        <span>
                            <i class="la la-close"></i>
                            <span>Annuler</span>
                        </span>
                    </button>
                </div>
            </div>
        </div>
    </div>
    <!--end modal-->
    <!--end::Entry-->
    <!--end::Content-->
