            <!--begin::Card-->
            <div class="card card-custom">
                <div class="card-header" style="background-color: #718E3E;">
                    <div class="card-title">
                        <span class="card-icon">
                            <i class="fas fa-book text-white icon-xl"></i>
                        </span>
                        <h3 class="card-title text-white" style="font-size: 30px; font-family: Maiandra GD">Document</h3>
                    </div>

                    <div style="margin-top:25px">
                        <button type="button" class="btn btn-outline-secondary btn-icon" data-toggle="modal" data-target="#modalVoirPlus" ng-click="">
                            <i class="fas fa-book text-white icon-xl"></i>
                        </button>
                        @*<span class="card-icon">
                            <i class="fas fa-book text-white icon-xl"></i>
                        </span>*@
                    </div>
                </div>
                <div class="card-body">
                    <!--begin::Row-->
                    <div class="row">
                        <div class="col-lg-4">
                            <!--begin::Card-->
                            <div class="card card-custom overlay">
                                <div class="card-body p-0">
                                    <div class="overlay-wrapper">
                                        
                                    </div>
                                    <div class="overlay-layer">
                                        <input ng-click="testPhoto()" type="file" class="btn font-weight-bold btn-light-primary btn-shadow ml-2" placeholder="Purchase" />
                                    </div>
                                </div>
                            </div>
                            <!--end::Card-->
                        </div>
                    </div>
                    <!--end::Row-->
                </div>
            </div>
