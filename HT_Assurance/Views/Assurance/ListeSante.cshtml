                <div class="card-body">
                    <!--begin: Search Form-->
                    <form class="mb-15" id="formListSante">
                        <div class="row mb-6">
                            <div class="col-lg-3 mb-lg-0 mb-6">
                                <label>Période*</label>
                                <div class="input-daterange input-group" id="">
                                    <input type="text" class="form-control datatable-input" name="listSante_PeriodeDu" placeholder="Du" data-col-index="5" ng-model="FormListSante.periodeDu"/>
                                    <div class="input-group-append">
                                        <span class="input-group-text">
                                            <i class="fas fa-arrows-alt-h text-info"></i>
                                        </span>
                                    </div>
                                    <input type="text" class="form-control datatable-input" name="listSante_PeriodeAu" placeholder="Au" data-col-index="5" ng-model="FormListSante.periodeAu"/>
                                </div>
                            </div>
                            <div class="col-lg-3 mb-lg-0 mb-6">
                                <label>N° police*</label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">
                                            <i class="fab fa-orcid text-info"></i>
                                        </span>
                                    </div>
                                    <input type="text" class="form-control datatable-input" name="listSante_NumPolice" placeholder="Entrer un numéro" data-col-index="4" ng-model="FormListSante.numPolice" />
                                </div>
                                <span class="form-text text-muted">Veuillez saisir le n° de la Police.</span>
                            </div>
                            <div class="col-lg-3 mb-lg-0 mb-6">
                                <label>N° Souscripteur*</label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">
                                            <i class="fab fa-orcid text-info"></i>
                                        </span>
                                    </div>
                                    <input type="text" class="form-control datatable-input" name="listSante_NumSouscripteur" placeholder="Entrer un numéro" data-col-index="4" ng-model="FormListSante.numSouscripteur" />
                                </div>
                                <span class="form-text text-muted">Veuillez sélectionner le n° du souscripteur.</span>
                            </div>
                            <div class="col-lg-3 mb-lg-0 mb-6">
                                <label>Nom du souscripteur*</label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">
                                            <i class="fas fa-address-card text-info"></i>
                                        </span>
                                    </div>
                                    <input type="text" class="form-control datatable-input" name="listSante_NomSouscripteur" placeholder="Entrer un nom" data-col-index="4" ng-model="FormListSante.nomSouscripteur" />
                                </div>
                                <span class="form-text text-muted">Veuillez saisir le nom du souscripteur.</span>
                            </div>
                        </div>
                        <div class="row mb-8">
                            <div class="col-lg-4 mb-lg-0 mb-6">
                                <label>N° Commercial*</label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">
                                            <i class="fab fa-orcid text-info"></i>
                                        </span>
                                    </div>
                                    <input type="text" class="form-control datatable-input" name="listSante_NumCommercial" placeholder="Entrer un n°" data-col-index="1" ng-model="FormListSante.numCommercial" />
                                </div>
                                <span class="form-text text-muted">Veuillez saisir le n° du commercial.</span>
                            </div>
                            <div class="col-lg-4 mb-lg-0 mb-6">
                                <label>Nom du Commercial*</label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">
                                            <i class="fas fa-address-card text-info"></i>
                                        </span>
                                    </div>
                                    <input type="text" class="form-control datatable-input" name="listSante_NomCommercial" placeholder="Entrer un nom" data-col-index="0" ng-model="FormListSante.nomCommercial"/>
                                </div>
                                <span class="form-text text-muted">Veuillez saisir le nom du commercial.</span>
                            </div>
                        </div>

                        <div class="row mt-8">
                            <div class="col-lg-12">
                                <button class="btn btn-primary btn-primary--icon" id="kt_search" ng-click="afficheListeSante(FormListSante.periodeDu,FormListSante.periodeAu,FormListSante.numPolice,FormListSante.numSouscripteur,FormListSante.nomSouscripteur,FormListSante.numCommercial,FormListSante.nomCommercial)">
                                    <span>
                                        <i class="la la-search"></i>
                                        <span>Réchercher</span>
                                    </span>
                                </button>&#160;&#160;
                                <button class="btn btn-outline-warning" id="kt_reset"  ng-click="listeSanteAnnuler()">
                                    <span>
                                        <i class="la la-close"></i>
                                        <span>Annuler</span>
                                    </span>
                                </button>
                            </div>
                        </div>
                    </form>
                    <!--begin: Datatable-->
                    <!--begin: Datatable-->
                    <table class="table table-bordered table-hover table-checkable" id="kt_datatable">
                        <thead style="text-align: center;">
                            <!--background-color: #8CB9BA; color: White-->
                            <tr>
                                <th>DATE</th>
                                <th>N° POLICE/N.C</th>
                                <th>SOUSCRIPTEUR</th>
                                <th>EFFET</th>
                                <th>ECHEANCE</th>
                                <th>STATUT CONTRAT</th>
                                <th>ACTIONS</th>
                            </tr>
                        </thead>
                        <tfoot>
                            <tr ng-repeat="laListeSante in listeSante">
                                <td>{{laListeSante.CA_DATESAISIE}}</td>
                                <td>{{laListeSante.CA_NUMPOLICE}}</td>
                                <td>N'est pas dans le retour</td>
                                <td>{{laListeSante.CA_DATEEFFET}}</td>
                                <td>{{laListeSante.CA_DATEECHEANCE}}</td>
                                <td>N'est pas dans le retour</td>
                                <!--ligne des actions-->
                                <td>
                                    <div class="example-preview" style="position: relative; right: -17px">
                                        <div class="btn-toolbar" role="toolbar" aria-label="...">
                                            <div class="btn-group mr-2" role="group" aria-label="...">
                                                <!--bouton voir plus-->
                                                <button type="button" class="btn btn-outline-secondary btn-icon" data-toggle="modal" data-target="#modalVoirPlus" ng-click="voirPlusSante(laListeSante)">
                                                    <i class="far fa-eye" data-toggle="tooltip" data-theme="dark" title="Voir plus"></i>
                                                </button>
                                                <!--bouton modifier-->
                                                <button id="addModif" type="button" class="btn btn-outline-secondary btn-icon" ng-click="voirModifSante(laListeSante)">
                                                    <i class="far fa-edit" data-toggle="tooltip" data-theme="dark" title="Modifier"></i>
                                                </button>
                                                <!--bouton supprimer-->
                                                <button id="swalSupprimer" type="button" class="btn btn-outline-secondary btn-icon" data-toggle="modal" data-target="#modalSupprimer" ng-click="supprRecupererIdElement(laListeSante.CA_CODECONTRAT)">
                                                     <i class="fas fa-ban" data-toggle="tooltip" data-theme="dark" title="Supprimer"></i>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </td>
                            </tr>
                        </tfoot>
                    </table>
                    <!--end: Datatable-->
                </div>
            </div>
            <!--end::Card-->
        </div>
        <!--end::Container-->
    </div>
    <!--end::Entry-->

    <!--begin modal pour voir plus-->
    <div class="modal fade" id="modalVoirPlus" tabindex="-1" role="dialog" aria-labelledby="modalVoirPlus" aria-hidden="true">
        <div class="modal-dialog modal-dialog-scrollable modal-lg modal-dialog-centered" role="document">
            <div class="modal-content">
                <div class="modal-header" style="background-color: #718E3E;">
                    <h5 class="modal-title text-white" id="modalVoirPlus" style="font-size: 23px; font-family: Maiandra GD">
                        <i class="far fa-eye text-white"></i>
                        Plus d'informations
                    </h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <i aria-hidden="true" class="ki ki-close text-white"></i>
                    </button>
                </div>
                <div class="modal-body" style="height: 300px;">
                    <div class="row col-lg-12">
                        <div class="col-lg-6">
                            <strong style="font-size: 13px; font-family: 'Maiandra GD'">DATE</strong> : {{recuperer.CA_DATESAISIE}} <br>
                            <strong style="font-size: 13px; font-family: 'Maiandra GD'">N° POLICE/N.C</strong> : {{recuperer.CA_NUMPOLICE}} <br>
                            <strong style="font-size: 13px; font-family: 'Maiandra GD'">SOUSCRIPTEUR</strong> : N'est pas dans le retour <br>
                        </div>
                        <div class="col-lg-6">
                            <strong style="font-size: 13px; font-family: 'Maiandra GD'">EFFET</strong> : {{recuperer.CA_DATEEFFET}} <br>
                            <strong style="font-size: 13px; font-family: 'Maiandra GD'">ECHEANCE</strong> : {{recuperer.CA_DATEECHEANCE}} <br>
                            <strong style="font-size: 13px; font-family: 'Maiandra GD'">STATUT CONTRAT</strong> : N'est pas dans le retour <br>
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-outline-info" data-dismiss="modal">
                        <span>
                            <i class="la la-close"></i>
                            <span>Fermer</span>
                        </span>
                    </button>
                </div>
            </div>
        </div>
    </div>
    <!--end modal-->
    <!--begin modal pour suppression-->
    <div class="modal fade" id="modalSupprimer" data-backdrop="static" tabindex="-1" role="dialog" aria-labelledby="modalSupprimer" aria-hidden="true">
        <div class="modal-dialog modal-xs modal-dialog-top" role="document">
            <div class="modal-content">
                <div class="modal-header" style="background-color: #718E3E;">
                    <h5 class="modal-title text-white" id="modalSupprimer" style="font-size: 23px; font-family: Maiandra GD">
                        <i class="fas fa-ban text-white"></i>
                        Contrat
                    </h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <i aria-hidden="true" class="ki ki-close text-white"></i>
                    </button>
                </div>
                <div class="modal-body" style="text-align: center; font-size: 17px;">
                    Etes-vous sûr de vouloir supprimer ce contrat ?
                </div>
                <div class="modal-footer">
                    <button class="btn btn-danger btn-shadow font-weight-bold" id="kt_reset"  data-dismiss="modal" ng-click="supprimeContrat(idElement)">
                        <span>
                            <i class="fas fa-ban"></i>
                            <span>Supprimer</span>
                        </span>
                    </button>
                    <button type="button" class="btn btn-outline-info" data-dismiss="modal">
                        <span>
                            <i class="la la-close"></i>
                            <span>Annuler</span>
                        </span>
                    </button>
                </div>
            </div>
        </div>
    </div>
    <!--end modal-->

</div>
<!--end::Content-->
