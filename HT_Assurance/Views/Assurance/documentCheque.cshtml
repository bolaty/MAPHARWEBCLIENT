            <!--begin::Card-->
            <div class="card card-custom">
                <div class="card-header" style="background-color: #718E3E;">
                    <div class="card-title">
                        <span class="card-icon">
                            <i class="fas fa-book text-white icon-xl"></i>
                        </span>
                        <h3 class="card-title text-white" style="font-size: 30px; font-family: Maiandra GD">Document</h3>
                    </div>
                    <div class="card-toolbar">
                        <i class="far fa-file-image text-white icon-xl" data-toggle="tooltip" title="Ajouter une image" data-placement="left" style="cursor: pointer;" ng-click="ajoutImage()"></i>
                    </div>
                </div>
                <div class="card-body">
                    <!--begin::Row-->
                    <div class="row">
                        <div class="col-lg-4" ng-model="img.upload" ng-repeat="lImage in image.img">
                            <!--begin::Card-->
                            <div class="col-lg-6" style="position: relative; left: 30px;">
                                <div class="image-input image-input-outline" id="kt_user_edit_avatar">
                                    <div class="image-input-wrapper-image" style="background-image: url(/Content/assets/media/illustr3.jpg)"></div>
                                    <div class="image-input-wrapper-image" style="position: absolute; top: 0; left: 0"><img ng-src="{{avatar}}" style="object-position: cover;" width="100%" height="100%" /></div>
                                    <label class="btn btn-xs btn-icon btn-circle btn-info btn-hover-text-primary btn-shadow" data-action="change" data-toggle="tooltip" title="" data-original-title="Nouvelle photo">
                                        <i class="fa fa-pen icon-sm text-white"></i>
                                        <input type="file" name="profile_avatar" accept=".png, .jpg, .jpeg" img-profile="FormAddCheque.img_avatar" onchange="angular.element(this).scope().previewPhoto(event)" />
                                        <input type="hidden" name="profile_avatar_remove" />
                                    </label>
                                    <span class="btn btn-xs btn-icon btn-circle btn-white btn-hover-text-primary btn-shadow" data-action="cancel" data-toggle="tooltip" title="Retirer la photo">
                                        <i class="ki ki-bold-close icon-xs text-muted"></i>
                                    </span>
                                </div>
                            </div>
                            <!--end::Card-->
                        </div>
                    </div>
                </div>
                <div class="card-footer">
                    <button type="submit" class="btn btn-primary btn-shadow-hover font-weight-bold" ng-click="ajoutFormAddGESA(FormAddGESA.effet, FormAddGESA.echeance)">
                        <span>
                            <i class="far fa-check-circle"></i>
                            <span>Enregistrer</span>
                        </span>
                    </button>
                    <a href="" class="btn btn-icon btn-light-dark mr-2" data-toggle="tooltip" data-theme="dark" title="Retirer la photo" style="position: absolute; left: 160px" ng-click="pourSupprimerAjoutDocumentCheque1()">
                        <i class="fas fa-undo"></i>
                    </a>
                    <a href="ListeGESA" class="btn btn-icon btn-light-dark mr-2" data-toggle="tooltip" data-theme="dark" title="Retour" style="position: absolute; right: 27px">
                        <i class="fas fa-arrow-left"></i>
                    </a>
                </div>
                    <!--end:ng-click="testPhoto()":Row-->
                </div>
            </div>
            <!--end::Card-->
        </div>
        <div class="modal fade" id="modalVoirPlus" tabindex="-1" role="dialog" aria-labelledby="modalVoirPlus" aria-hidden="true">
            <div class="modal-dialog modal-dialog-scrollable modal-lg modal-dialog-centered" role="document">
                <div class="modal-content">
                    <div class="modal-header" style="background-color: #718E3E;">
                        <h5 class="modal-title text-white" id="modalVoirPlus" style="font-size: 23px; font-family: Maiandra GD">
                            <i class="far fa-eye text-white"></i>
                            Photo
                        </h5>
                        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <i aria-hidden="true" class="ki ki-close text-white"></i>
                        </button>
                    </div>
                    <div class="modal-body" style="height: 600px;">
                        <div class="card-body p-0">
                            <div class="overlay-wrapper">
                                <img src="~/content/assets/media/illustr3.jpg" height="400" alt="" class="w-100 rounded" />
                            </div>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-outline-info" data-dismiss="modal">
                            <span>
                                <i class="la la-close"></i>
                                <span>Fermer</span>
                            </span>
                        </button>
                    </div>
                </div>
            </div>
