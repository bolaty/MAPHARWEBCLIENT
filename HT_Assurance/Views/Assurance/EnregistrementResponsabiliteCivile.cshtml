@{
    ViewBag.Title = "Enregistrement Responsabilite Civile";
    Layout = "~/Views/Shared/_LayoutMenuGestion.cshtml";
}


<!--begin::Content-->
<div class="content d-flex flex-column flex-column-fluid" id="kt_content" ng-controller="ResponsabiliteCivileController" ng-init="initFormAddResponsabiliteCivile()">
