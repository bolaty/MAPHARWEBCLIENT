@{
ViewBag.Title = "Opération Multirisque professionnel";
Layout = "~/Views/Shared/_LayoutMenuGestion.cshtml";
}



<!--begin::Content-->
<div class="content d-flex flex-column flex-column-fluid" id="kt_content">
    <!--begin::Subheader-->
    <div class="subheader py-2 py-lg-6 subheader-solid" id="kt_subheader" style="background: Beige">
        <div class="container-fluid d-flex align-items-center justify-content-between flex-wrap flex-sm-nowrap">
            <!--begin::Info-->
            <div class="d-flex align-items-center flex-wrap mr-1">
                <!--begin::Page Heading-->
                <div class="d-flex align-items-baseline flex-wrap mr-5">
                    <!--begin::Page Title-->
                   @* <h3 class="display-5">Lettrage</h3>*@
                  <h5 class="text-dark font-weight-bold my-1 mr-5">GESTION</h5>&nbsp;&nbsp;
                    <!--end::Page Title-->
                    <!--begin::Breadcrumb-->
                    <ul class="breadcrumb breadcrumb-transparent breadcrumb-dot font-weight-bold p-0 my-2 font-size-sm">
                        <li class="breadcrumb-item">
                            <a href="" class="text-muted">Comptabilité</a>
                        </li>
                        <li class="breadcrumb-item">
                            <a href="" class="text-muted">liste</a>
                        </li>
                        <li class="breadcrumb-item">
                            <a href="" class="text-muted">Lettrage</a>
                        </li>
                    </ul>
                    <!--end::Breadcrumb-->
                </div>
                <!--end::Page Heading-->
            </div>
            <!--end::Info-->
            <!--begin::Toolbar-->
            <div class="d-flex align-items-center">
                <!--begin::Actions-->
               
                <!--end::Actions-->
                <!--begin::Dropdown-->
              
                <!--end::Dropdown-->
            </div>
            <!--end::Toolbar-->
        </div>
    </div>
    <!--end::Subheader-->
    <!--begin::Entry-->
    <div class="d-flex flex-column-fluid">
        <!--begin::Container-->
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <!--begin::Card-->
                    <div class="card card-custom gutter-b example example-compact">
                        <div class="card-header" style="background-color: #F5F500">
                            <div class="card-title">
                                <span class="card-icon">
                                    <i class="fas fa-shopping-cart text-dark icon-xl"></i>
                                </span>
                                <h3 class="card-title" style="font-size: 30px; font-family: 'Maiandra GD'">Lettrage</h3>
                            </div>
                        </div>
                        <!--begin::Form-->
                        <form class="form">
                            <div class="card-body">
                                <div class="row">
                                    <div class="col-12">
                                        <div class="mb-3">
                                            @*<h3 class="font-size-lg text-dark-75 font-weight-bold mb-10">Recherche Piece Comptable</h3>*@                                           
                                            <div class="h4 text-dark-75"><strong>Recherche Piece Comptable :</strong> </div><br />
                                            <div class="mb-2">
                                                <div class="form-group row">
                                                    <div class="col-lg-6" style="margin-top:20px">
                                                        <label>Exercice*</label>
                                                        <div class="input-group">
                                                            <div class="input-group-prepend">
                                                                <span class="input-group-text">
                                                                    <i class="fab fa-etsy text-info"></i>
                                                                </span>
                                                            </div>
                                                            <select class="form-control" name="option">
                                                                <option value="">Sélectionner un exercice</option>
                                                                <option>2020</option>
                                                                <option>2021</option>
                                                                <option>2022</option>
                                                                <option>2023</option>
                                                                <option>2024</option>
                                                                <option>2025</option>
                                                            </select>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6" style="margin-top:20px">
                                                        <label>Periodicité*</label>
                                                        <div class="input-group">
                                                            <div class="input-group-prepend">
                                                                <span class="input-group-text">
                                                                    <i class="far fa-hourglass text-info"></i>
                                                                </span>
                                                            </div>
                                                            <select class="form-control" name="option">
                                                                <option value="">Sélectionner une periodicité</option>
                                                                <option>periodicité 1</option>
                                                                <option>periodicité 2</option>
                                                                <option>periodicité 3</option>
                                                                <option>periodicité 4</option>
                                                                <option>periodicité 5</option>
                                                            </select>
                                                        </div>
                                                        <span class="form-text text-muted">Veuillez sélectionner la periodicité.</span>
                                                    </div>
                                                    <div class="col-lg-6" style="margin-top:20px">
                                                        <label>* Periode:</label>
                                                        <div class="input-group">
                                                            <div class="input-group-prepend">
                                                                <span class="input-group-text">
                                                                    <i class="far fa-calendar-check text-info"></i>
                                                                </span>
                                                            </div>
                                                            <select class="form-control" name="option">
                                                                <option value="">Sélectionner une Periode</option>
                                                                <option>Janvier</option>
                                                                <option>Janvier 2</option>
                                                                <option>Janvier 3</option>
                                                                <option>Janvier 4</option>
                                                                <option>Janvier 5</option>
                                                            </select>
                                                        </div>
                                                        <span class="form-text text-muted">Veuillez sélectionnez une Periode.</span>
                                                    </div>
                                                    <div class="col-lg-3" style="margin-top:25px">
                                                        <label>* Date debut:</label>
                                                       
                                                        <div class="input-group">
                                                            <div class="input-group-prepend">
                                                                <span class="input-group-text">
                                                                    <i class="far fa-calendar-alt text-info"></i>
                                                                </span>
                                                            </div>
                                                            <input type="text" class="form-control" name="url" placeholder="Entrer la Date debut" />
                                                        </div>
                                                        <span class="form-text text-muted">Veuillez saisir la Date debut.</span>
                                                    </div>
                                                    <div class="col-lg-3" style="margin-top:25px">
                                                        <label>* Date fin:</label>
                                                        <div class="input-group">
                                                            <div class="input-group-prepend">
                                                                <span class="input-group-text">
                                                                    <i class="far fa-calendar-alt text-info"></i>
                                                                </span>
                                                            </div>
                                                            <input type="text" class="form-control" name="url" placeholder="Entrer le numéro de la Date fin" />
                                                        </div>
                                                        <span class="form-text text-muted">Veuillez saisir le numéro de la Date fin.</span>
                                                    </div>
                                                    <div class="col-lg-4">
                                                        <label>* Compte general:</label>
                                                        <div class="input-group">
                                                            <div class="input-group-prepend">
                                                                <span class="input-group-text">
                                                                    <i class="fas fa-landmark text-info"></i>
                                                                </span>
                                                            </div>
                                                            <input type="text" class="form-control" name="url" placeholder="Entrer le Compte general" />
                                                            
                                                        </div>
                                                        
                                                    </div>
                                                    <div class="col-lg-2 mb-lg-0 mb-6" style="position: relative; top: 25px">
                                                        <button class="btn btn-secondary btn-secondary--icon" id="kt_reset">
                                                            ...
                                                        </button>
                                                    </div>
                                                    <div class="col-lg-4">
                                                        <label>*Compte tiers:</label>
                                                        <div class="input-group">
                                                            <div class="input-group-prepend">
                                                                <span class="input-group-text">
                                                                    <i class="fas fa-landmark text-info"></i>
                                                                </span>
                                                            </div>
                                                            <input type="text" class="form-control" name="url" placeholder="Entrer Compte tiers" />
                                                           
                                                        </div>

                                                    </div>
                                                    <div class="col-lg-2 mb-lg-0 mb-6" style="position: relative; top: 25px">
                                                        <button class="btn btn-secondary btn-secondary--icon" id="kt_reset">
                                                            ...
                                                        </button>
                                                    </div>
                                                    <div class="col-lg-6" style="margin-top:25px">
                                                        <label>* Lettre:</label>
                                                        <div class="input-group">
                                                            <div class="input-group-prepend">
                                                                <span class="input-group-text">
                                                                    <i class="fab fa-etsy text-info"></i>
                                                                </span>
                                                            </div>
                                                            <select class="form-control" name="option">
                                                                <option value="">A</option>
                                                                <option>Mr le Directeur</option>
                                                                <option>2</option>
                                                                <option>3</option>
                                                                <option>4</option>
                                                                <option>5</option>
                                                            </select>
                                                        </div>
                                                            <span class="form-text text-muted">Veuillez sélectionnez un destinataire.</span>
                                                        </div>
                                                    <div class="col-lg-3" style="margin-top:25px">
                                                        <label></label>
                                                        <div class="input-group">
                                                            <input type="text" class="form-control" name="url" placeholder="Charger Ecriture" disabled/>
                                                        </div>
                                                        
                                                    </div>
                                                    <div class="col-lg-3" style="margin-top:25px">
                                                        <label></label>
                                                        <div class="input-group">
                                                            <input type="text" class="form-control" name="url" placeholder="Appliquer le lettrage" disabled/>
                                                        </div>
                                                        
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="separator separator-dashed my-10"></div>
                            <div class="card-body">
                                <div class="row">
                                    <div class="col-12">
                                        <div class="mb-3">
                                            @*<h3 class="font-size-lg text-dark-75 font-weight-bold mb-10"> OPERATION :</h3>*@
                                            <div class="h4 text-dark-75"><strong> Operation :</strong></div><br />
                                            <div class="">
                                                <div class="card card-custom mb-3">
                                                    <div class="card-body">
                                                        <!--begin: Datatable-->
                                                        <div class="row">
                                                            <div class="col-lg-12">
                                                                <table class="table table-bordered table-checkable" id="kt_datatable">
                                                                    <thead style="text-align: center;">
                                                                        <tr>
                                                                            <th>DATE PIECE</th>
                                                                            <th>CPT GENERAL</th>
                                                                            <th>CPT TIERS </th>
                                                                            <th>REFERENCE PIECE</th>
                                                                            <th>LIBELLE OPERATION</th>
                                                                            <th>DEBIT</th>
                                                                            <th>CREDIT</th>
                                                                        </tr>
                                                                    </thead>
                                                                    <tbody>
                                                                        <tr>
                                                                            <td>   </td>
                                                                            <td>   </td>
                                                                            <td>   </td>
                                                                            <td>   </td>
                                                                            <td>   </td>
                                                                            <td>   </td>
                                                                            <td>   </td>
                                                                        </tr>
                                                                        <tr>
                                                                            <td colspan="5"></td>
                                                                            <td>0</td>
                                                                            <td>0</td>
                                                                        </tr>
                                                                    </tbody>
                                                                </table>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <!--end: Datatable-->
                                                </div>

                                            </div>

                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!--begin::Code example example-highlight  -->
                            <div class="card-footer"> 
                                <button class="btn btn-outline-success" id="kt_reset">
                                    <span>
                                        <i class="fas fa-circle"></i>
                                        <span>Lettrer</span>
                                    </span>
                                </button>                                                      
                                <button class="btn btn-danger btn-shadow font-weight-bold" id="kt_reset">
                                    <span>
                                        <i class="fas fa-ban"></i>
                                        <span>Supprimer lettrage</span>
                                    </span>
                                </button>                             
                                <button class="btn btn-outline-warning" id="kt_reset">
                                    <span>
                                        <i class="la la-close"></i>
                                        <span>Annuler</span>
                                    </span>
                                </button>
                            </div>
                        </form>
                        <!--end::Form-->
                    </div>
                    <!--end::Card-->
                </div>
            </div>
        </div>
        <!--end::Container-->
    </div>
    <!--end::Entry-->
</div>
<!--end::Content-->
