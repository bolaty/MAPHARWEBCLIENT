@{
    ViewBag.Title = "Habitat";
Layout = "~/Views/Shared/_LayoutMenuGestion.cshtml";
}


<!--begin::Content-->
<div class="content d-flex flex-column flex-column-fluid" id="kt_content">
    <!--begin::Subheader-->
    <div class="subheader py-2 py-lg-6 subheader-solid" id="kt_subheader" style="background: Beige">
        <div class="container-fluid d-flex align-items-center justify-content-between flex-wrap flex-sm-nowrap">
            <!--begin::Info-->
            <div class="d-flex align-items-center flex-wrap mr-1">
                <!--begin::Page Heading-->
                <div class="d-flex align-items-baseline flex-wrap mr-5">
                    <!--begin::Page Title-->
                    <h5 class="text-dark font-weight-bold my-1 mr-5">GESTION</h5>&nbsp;&nbsp;
                    @*<h5 class="text-dark font-weight-bold my-1 mr-5">Recherche de pièce comptable</h5>*@
                    <!--end::Page Title-->
                    <!--begin::Breadcrumb-->
                    <ul class="breadcrumb breadcrumb-transparent breadcrumb-dot font-weight-bold p-0 my-2 font-size-sm">
                        <li class="breadcrumb-item">
                            <a href="" class="text-muted">Comptabilité</a>
                        </li>
                        <li class="breadcrumb-item">
                            <a href="" class="text-muted">Liste</a>
                        </li>
                        <li class="breadcrumb-item">
                            <a href="" class="text-muted">Recherche de pièce comptable</a>
                        </li>
                    </ul>
                    <!--end::Breadcrumb-->
                </div>
                <!--end::Page Heading-->
            </div>
            <!--end::Info-->
            <!--begin::Toolbar-->
           
            <!--end::Toolbar-->
        </div>
    </div>
    <!--end::Subheader-->
    <!--begin::Entry-->
    <div class="d-flex flex-column-fluid">
        <!--begin::Container-->
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <!--begin::Card-->
                    <div class="card card-custom gutter-b example example-compact">
                        <div class="card-header" style="background-color: #F5F500">

                            <div class="card-title">
                                <span class="card-icon">
                                    <i class="fas fa-shopping-cart text-dark icon-xl"></i>
                                </span>
                                <h3 class="card-title" style="font-size: 30px; font-family: 'Maiandra GD'">Recherche de pièce comptable</h3>
                            </div>
                        </div>
                        <!--begin::Form-->
                        <form class="form">
                            <div class="card-body">
                                <div class="row">
                                    <div class="col-12">
                                        <div class="mb-3">
                                            <div class="h4 text-dark-75">Recherche Piece Comptable :</div><br />
                                            @*<h3 class="font-size-lg text-dark-75 font-weight-bold mb-10">Recherche Piece Comptable</h3>*@
                                            <div class="mb-2">
                                                <div class="form-group row">
                                                    <div class="col-lg-3">
                                                        <label> N° Piece*:</label>
                                                        <div class="input-group">
                                                            <div class="input-group-prepend">
                                                                <span class="input-group-text">
                                                                    <i class="fab fa-orcid text-info"></i>
                                                                </span>
                                                            </div>
                                                            <input type="text" class="form-control" name="url" placeholder="Entrer le numéro de la Piece" />
                                                        </div>
                                                    </div>

                                                    <div class="col-lg-4">
                                                        <label> Reference*</label>
                                                        <div class="input-group">
                                                            <div class="input-group-prepend">
                                                                <span class="input-group-text">
                                                                    <i class="fas fa-barcode text-info"></i>
                                                                </span>
                                                            </div>
                                                            <input type="text" class="form-control" name="url" placeholder="Entrer la Reference" />
                                                        </div>

                                                    </div>
                                                    <div class="col-lg-5">
                                                        <label>Libelle Operation*</label>
                                                        <div class="input-group">
                                                            <div class="input-group-prepend">
                                                                <span class="input-group-text">
                                                                    <i class="fas fa-address-card text-info"></i>
                                                                </span>
                                                            </div>
                                                            <input type="text" class="form-control" name="url" placeholder="Entrer le Libelle de l'Operation" />
                                                        </div>
                                                    </div>

                                                    <div class="col-lg-2" style="margin-top:20px">
                                                        <label> Entrepot*:</label>
                                                        <div class="input-group">
                                                            <div class="input-group-prepend">
                                                                <span class="input-group-text">
                                                                    <i class="fas fa-warehouse text-info"></i>
                                                                </span>
                                                            </div>
                                                            <select class="form-control" name="option">
                                                                <option value="">A</option>
                                                                <option>Selectionner un Entrepot</option>
                                                                <option>2</option>
                                                                <option>3</option>
                                                                <option>4</option>
                                                                <option>5</option>
                                                            </select>
                                                        </div>
                                                        @*<span class="form-text text-muted">Veuillez sélectionnez un annee.</span>*@
                                                    </div>
                                                    <div class="col-lg-4" style="margin-left:5px;margin-top:25px">
                                                        <label>Période*</label>
                                                        <div class="input-daterange input-group" id="kt_datepicker">
                                                            <input type="text" class="form-control datatable-input" name="start" placeholder="Du" data-col-index="5" />
                                                            <div class="input-group-append">
                                                                <span class="input-group-text">
                                                                    <i class="fas fa-arrows-alt-h text-info"></i>
                                                                </span>
                                                            </div>
                                                            <input type="text" class="form-control datatable-input" name="end" placeholder="Au" data-col-index="5" />
                                                        </div>
                                                    </div>
                                                    
                                                    <div class="col-lg-2" style="margin-left:5px;margin-top:25px">
                                                        <label>* Montant debut:</label>
                                                        <div class="input-group">
                                                            <div class="input-group-prepend">
                                                                <span class="input-group-text">
                                                                    <i class="fas fa-money-bill text-info"></i>
                                                                </span>
                                                            </div>
                                                            <input type="text" class="form-control" name="url" placeholder="Entrer le numéro du Bordereau" />
                                                        </div>
                                                        @*<span class="form-text text-muted">Veuillez saisir de numéro du Bordereau.</span>*@
                                                    </div>
                                                    <div class="col-lg-3" style="margin-left:5px;margin-top:25px">
                                                        <label>* Montant fin:</label>
                                                        <div class="input-group">
                                                            <div class="input-group-prepend">
                                                                <span class="input-group-text">
                                                                    <i class="fas fa-money-bill text-info"></i>
                                                                </span>
                                                            </div>
                                                            <input type="text" class="form-control" name="url" placeholder="Entrer la Date debut" />
                                                        </div>
                                                        @*<span class="form-text text-muted">Veuillez saisir la Date debut.</span>*@
                                                    </div>

                                                    <div class="col-lg-3" style="margin-top:25px;margin-left:50px;width:20px">
                                                        <label class="checkbox checkbox-lg checkbox-primary flex-shrink-0 m-0 mr-4">
                                                            Debit&nbsp;&nbsp;
                                                            <input type="checkbox" value="Due in 1 Year" />
                                                            <span class="blue"></span>
                                                        </label>
                                                    </div>
                                                    <div class="col-lg-3" style="margin-top:35px;margin-left:65px;width:20px">
                                                        <label class="checkbox checkbox-lg checkbox-primary flex-shrink-0 m-0 mr-4">
                                                            Credit&nbsp;&nbsp;
                                                            <input type="checkbox" value="Due in 1 Year" />
                                                            <span class="blue"></span>
                                                        </label>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-lg-3" style="margin-top:45px">
                                                <button class="btn btn-primary btn-primary--icon" id="kt_search">
                                                    <span>
                                                        <i class="la la-search"></i>
                                                        <span>Rechercher</span>
                                                    </span>
                                                </button>
                                                <button class="btn btn-outline-warning" id="kt_reset">
                                                    <span>
                                                        <i class="la la-close"></i>
                                                        <span>Annuler</span>
                                                    </span>
                                                </button>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="separator separator-dashed my-10"></div>
                            <div class="card-body">
                                <div class="row">
                                    <div class="col-12">
                                        <div class="mb-3">
                                            @*<h3 class="font-size-lg text-dark-75 font-weight-bold mb-10"> RESULTAT :</h3>*@
                                            <div class="h4 text-dark-75">Resultat :</div><br />
                                            <div class="">
                                                <div class="card card-custom mb-3">
                                                    <div class="card-body">
                                                        <!--begin: Datatable-->
                                                        <div class="row">
                                                            <div class="col-lg-12">
                                                                <table class="table table-bordered table-checkable" id="kt_datatable">
                                                                    <thead style="text-align: center;">
                                                                        <tr>
                                                                            <th>REFERENCE</th>
                                                                            <th>N° PIECE </th>
                                                                            <th>DATE PIECE</th>
                                                                            <th>N° COMPTE</th>
                                                                            <th>N° COMPTE TIERS</th>
                                                                            <th>LIBELLE MOUVEMENT</th>
                                                                            <th>DEBIT</th>
                                                                            <th>CREDIT</th>

                                                                        </tr>
                                                                    </thead>
                                                                    <tbody>
                                                                        <tr>
                                                                            <td>   </td>
                                                                            <td>   </td>
                                                                            <td>   </td>
                                                                            <td>   </td>
                                                                            <td>   </td>
                                                                            <td>   </td>
                                                                            <td>   </td>
                                                                            <td>   </td>
                                                                        </tr>
                                                                        <tr>
                                                                            <td colspan="6"></td>
                                                                            <td>0</td>
                                                                            <td>0</td>

                                                                        </tr>
                                                                    </tbody>
                                                                </table>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <!--end: Datatable-->
                                                </div>

                                            </div>

                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="separator separator-dashed my-10"></div>
                            <!--begin::Code example example-highlight  -->

                            <div class="card-footer">
                                <button type="submit" class="btn btn-primary btn-shadow-hover font-weight-bold">
                                    <span>
                                        <i class="far fa-check-circle"></i>
                                        <span>Enregistrer</span>
                                    </span>
                                </button>
                                <button class="btn btn-outline-warning" id="kt_reset">
                                    <span>
                                        <i class="la la-close"></i>
                                        <span>Annuler</span>
                                    </span>
                                </button>
                            </div>
                        </form>
                        <!--end::Form-->
                    </div>
                    <!--end::Card-->
                </div>
            </div>
        </div>
        <!--end::Container-->
    </div>
    <!--end::Entry-->
</div>
<!--end::Content-->
