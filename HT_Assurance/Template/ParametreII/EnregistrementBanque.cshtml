@{
    ViewBag.Title = "Enregistrement Banque";
    Layout = "~/Views/Shared/_LayoutMenuGestion.cshtml";
}


<!--begin::Content-->
<div class="content d-flex flex-column flex-column-fluid" id="kt_content">
    <!--begin::Subheader-->
    <div class="subheader py-2 py-lg-6 subheader-solid" id="kt_subheader" style="background-color: beige">
        <div class="container-fluid d-flex align-items-center justify-content-between flex-wrap flex-sm-nowrap">
            <!--begin::Info-->
            <div class="d-flex align-items-center flex-wrap mr-1">
                <!--begin::Page Heading-->
                <div class="d-flex align-items-baseline flex-wrap mr-5">
                    <!--begin::Page Title-->
                    @*<h5 class="text-dark font-weight-bold my-1 mr-5">Cloture d'exercice</h5>*@
                    <h3 class="text-dark font-weight-bold my-1 mr-5">GESTION</h3>

                    <!--end::Page Title-->
                    <!--begin::Breadcrumb-->
                    <ul class="breadcrumb breadcrumb-transparent breadcrumb-dot font-weight-bold p-0 my-2 font-size-sm">
                        <li class="breadcrumb-item">
                            <a href="" class="text-muted">ParamètreII</a>
                        </li>
                        <li class="breadcrumb-item">
                            <a href="" class="text-muted">Enregistrement</a>
                        </li>
                        <li class="breadcrumb-item">
                            <a href="" class="text-muted">Banque</a>
                        </li>
                    </ul>
                    <!--end::Breadcrumb-->
                </div>
                <!--end::Page Heading-->
            </div>
            <!--end::Info-->
            <!--begin::Toolbar-->
            <!--end::Toolbar-->
        </div>
    </div>
    <!--end::Subheader-->
    <!--begin::Entry-->
    <div class="d-flex flex-column-fluid">
        <!--begin::Container-->
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <!--begin::Card-->
                    <div class="card card-custom gutter-b example example-compact">
                        <div class="card-header" style="background-color: #F5F500;">
                            <div class="card-title">
                                <span class="card-icon">
                                    <i class="fas fa-landmark text-dark icon-xl"></i>
                                </span>
                                <h3 class="card-title" style="font-size: 30px; font-family: 'Maiandra GD'">Banque</h3>
                            </div>
                        </div>
                        <!--begin::Form-->
                        <form class="form">
                            <div class="card-body">
                                <div class="row">
                                    <div class="col-lg-6">
                                        <label>Code*</label>
                                        <div class="input-group">
                                            <div class="input-group-prepend">
                                                <span class="input-group-text">
                                                    <i class="fas fa-key text-info"></i>
                                                </span>
                                            </div>
                                            <input type="text" class="form-control" name="url" placeholder="Entrer un code" />
                                        </div>
                                        <span class="form-text text-muted">Veuillez saisir le code.</span>
                                    </div>

                                    <div class="col-lg-6">
                                        <label>Sigle*</label>
                                        <div class="input-group">
                                            <div class="input-group-prepend">
                                                <span class="input-group-text">
                                                    <i class="far fa-save text-info"></i>
                                                </span>
                                            </div>
                                            <input type="text" class="form-control" name="url" placeholder="Entrer un sigle" />
                                        </div>
                                        <span class="form-text text-muted">Veuillez saisir le sigle.</span>
                                    </div>
                                </div><div class="row">
                                    <div class="col-lg-12">
                                        <label>Raison sociale*</label>
                                        <div class="input-group">
                                            <div class="input-group-prepend">
                                                <span class="input-group-text">
                                                    <i class="fab fa-rust text-info"></i>
                                                </span>
                                            </div>
                                            <input type="text" class="form-control" name="url" placeholder="Entrer une raison sociale" />
                                        </div>
                                        <span class="form-text text-muted">Veuillez saisir la raison sociale.</span>
                                    </div>
                                </div>
                            </div>
                            <div class="separator separator-dashed separator-success"></div>
                            <div class="card-body">
                                <div class="row">
                                    <div class="col-12">
                                        <div class="mb-3">
                                            <table class="table table-bordered table-hover table-checkable" id="kt_datatable">
                                                <thead style="text-align: center;">
                                                    <!--background-color: #8CB9BA; color: White-->
                                                    <tr>
                                                        <th>NOM D'AGENCE</th>
                                                        <th>ADRESSE GEOGRAPHIQUE</th>
                                                        <th>BOITE POSTALE</th>
                                                        <th>EMAIL</th>
                                                        <th>TELEPHONE</th>
                                                        <th>FAX</th>
                                                        <th>AB</th>
                                                        <th>CO</th>
                                                        <th>SIT</th>
                                                        <th>AUT</th>
                                                    </tr>
                                                </thead>
                                                <tfoot>
                                                    <tr>
                                                        <th></th>
                                                        <th></th>
                                                        <th></th>
                                                        <th></th>
                                                        <th></th>
                                                        <th></th>
                                                        <th></th>
                                                        <th></th>
                                                        <th></th>
                                                        <th></th>
                                                    </tr>
                                                </tfoot>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                                <div class="row mt-8">
                                    <div class="col-lg-12">
                                        <button type="submit" class="btn btn-primary btn-shadow-hover font-weight-bold">
                                            <span>
                                                <i class="far fa-check-circle"></i>
                                                <span>Enregistrer</span>
                                            </span>
                                        </button>
                                        <button type="submit" class="btn btn-light-success font-weight-bold">
                                            <span>
                                                <i class="fas fa-undo"></i>
                                                <span>Réinitialiser</span>
                                            </span>
                                        </button>
                                        <a href="Banque" class="btn btn-icon btn-light-dark mr-2" data-toggle="tooltip" data-theme="dark" title="Retour" style="position: absolute; right: 27px">
                                            <i class="fas fa-arrow-left"></i>
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </form>
                        <!--end::Form-->
                        <!--end::Card-->
                    </div>
                </div>
            </div>
        </div>
        <!--end::Container-->
    </div>
    <!--end::Entry-->
</div>
<!--end::Content-->
