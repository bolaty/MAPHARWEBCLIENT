                    <form class="mb-15">
                        <div class="row mb-6">
                            <div class="col-lg-3 mb-lg-0 mb-6">
                                <label>Période*</label>
                                <div class="input-daterange input-group" id="kt_datepicker">
                                    <input type="text" class="form-control datatable-input" name="start" placeholder="Du" data-col-index="5" />
                                    <div class="input-group-append">
                                        <span class="input-group-text">
                                            <i class="fas fa-arrows-alt-h text-info"></i>
                                        </span>
                                    </div>
                                    <input type="text" class="form-control datatable-input" name="Du" placeholder="Au" data-col-index="5" />
                                </div>
                            </div>
                            <div class="col-lg-3 mb-lg-0 mb-6">
                                <label>N° police*</label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">
                                            <i class="fab fa-orcid text-info"></i>
                                        </span>
                                    </div>
                                    <input type="text" class="form-control datatable-input" placeholder="Entrer le numéro de la police" data-col-index="1" />
                                </div>
                            </div>
                            <div class="col-lg-3 mb-lg-0 mb-6">
                                <label>N° Souscripteur*</label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">
                                            <i class="fab fa-orcid text-info"></i>
                                        </span>
                                    </div>
                                    <input type="text" class="form-control datatable-input" placeholder="Entrer le numéro du souscripteur" data-col-index="1" />
                                </div>
                            </div>
                            <div class="col-lg-3 mb-lg-0 mb-6">
                                <label>Nom du Souscripteur*</label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">
                                            <i class="fas fa-address-card text-info"></i>
                                        </span>
                                    </div>
                                    <input type="text" class="form-control datatable-input" placeholder="Entrer le nom du souscripteur" data-col-index="4" />
                                </div>
                            </div>
                        </div>
                        <div class="row mb-8">
                            <div class="col-lg-3 mb-lg-0 mb-6">
                                <label>N° Commercial*</label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">
                                            <i class="fas fa-key text-info"></i>
                                        </span>
                                    </div>
                                    <input type="text" class="form-control datatable-input" placeholder="Entrer le numéro du commercial" data-col-index="1" />
                                </div>
                            </div>
                            <div class="col-lg-3 mb-lg-0 mb-6">
                                <label>Nom du Commercial*</label>
                                <div class="input-group">
                                    <div class="input-group-prepend">
                                        <span class="input-group-text">
                                            <i class="fas fa-address-card text-info"></i>
                                        </span>
                                    </div>
                                    <input type="text" class="form-control datatable-input" placeholder="Entrer le nom du commercial" data-col-index="0" />
                                </div>
                            </div>
                            <div class="col-lg-3 mb-lg-0 mb-6" style="position: relative; top: 25px">
                                <button class="btn btn-secondary btn-secondary--icon" id="kt_reset">
                                    <strong>...</strong>
                                </button>
                            </div>
                        </div>
                        <div class="row mt-8">
                            <div class="col-lg-12">
