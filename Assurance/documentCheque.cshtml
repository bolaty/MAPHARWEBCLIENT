        </div>
        <!--end::Container-->
    </div>
    <!--end::Entry-->
    <div class="modal fade" id="modalVoirPlus" tabindex="-1" role="dialog" aria-labelledby="modalVoirPlus" aria-hidden="true">
        <div class="modal-dialog modal-dialog-scrollable modal-lg modal-dialog-centered" role="document">
            <div class="modal-content">
                <div class="modal-header" style="background-color: #718E3E;">
                    <h5 class="modal-title text-white" id="modalVoirPlus" style="font-size: 23px; font-family: Maiandra GD">
                        <i class="far fa-eye text-white"></i>
                        Plus d'informations
                    </h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <i aria-hidden="true" class="ki ki-close text-white"></i>
                    </button>
                </div>
                <div class="modal-body" style="height: 300px;">
                    <div class="row col-lg-12">
                        <div class="col-lg-6">
                            <strong style="font-size: 13px; font-family: 'Maiandra GD'">REFERENCE CHEQUE</strong> : {{recuperer.CH_REFCHEQUE}} <br>
                            <strong style="font-size: 13px; font-family: 'Maiandra GD'">NOM DEPOSANT</strong> : {{recuperer.CH_NOMDUDEPOSANT}} <br>
                            <strong style="font-size: 13px; font-family: 'Maiandra GD'">TELEPHONE DEPOSANT</strong> :{{recuperer.CH_TELEPHONEDEPOSANT}} <br>
                            <strong style="font-size: 13px; font-family: 'Maiandra GD'">BANQUE</strong> : banque <br>
                        </div>
                        <div class="col-lg-6">
                            <strong style="font-size: 13px; font-family: 'Maiandra GD'">MONTANT</strong> : montant <br>
                            <strong style="font-size: 13px; font-family: 'Maiandra GD'">DATE VALIDATION EFFECTIVE</strong> : {{recuperer.CA_DATEECHEANCE}} <br>
                            <strong style="font-size: 13px; font-family: 'Maiandra GD'">DATE D'EFFET</strong> : {{recuperer.CH_DATEEFFET}}
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-outline-info" data-dismiss="modal">
                        <span>
                            <i class="la la-close"></i>
                            <span>Fermer</span>
                        </span>
                    </button>
                </div>
            </div>
        </div>
    </div>
    <!--end modal-->
    <!--begin modal pour suppression-->
    <div class="modal fade" id="modalSupprimer" data-backdrop="static" tabindex="-1" role="dialog" aria-labelledby="modalSupprimer" aria-hidden="true">
        <div class="modal-dialog modal-xs modal-dialog-top" role="document">
            <div class="modal-content">
                <div class="modal-header" style="background-color: #718E3E;">
                    <h5 class="modal-title text-white" id="modalSupprimer" style="font-size: 23px; font-family: Maiandra GD">
                        <i class="fas fa-ban text-white"></i>
                        Contrat
                    </h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <i aria-hidden="true" class="ki ki-close text-white"></i>
                    </button>
                </div>
                <div class="modal-body" style="text-align: center; font-size: 17px;">
                    Etes-vous sûr de vouloir supprimer ce contrat ?
                </div>
                <div class="modal-footer">
                    <button class="btn btn-danger btn-shadow font-weight-bold" id="kt_reset" ng-click="supprimerCheque()" data-dismiss="modal">
                        <span>
                            <i class="fas fa-ban"></i>
                            <span>Supprimer</span>
                        </span>
                    </button>
                    <button type="button" class="btn btn-outline-info" data-dismiss="modal">
                        <span>
                            <i class="la la-close"></i>
                            <span>Annuler</span>
                        </span>
                    </button>
                </div>
            </div>
        </div>
    </div>
    <!--end modal-->
    <!--end::Entry-->
<!--end::Content-->

