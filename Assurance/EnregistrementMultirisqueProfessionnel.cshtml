                                                    <div class="col-lg-6">
                                                        <label>N° police/ N.C*</label>
                                                        <div class="input-group">
                                                            <div class="input-group-prepend">
                                                                <span class="input-group-text">
                                                                    <i class="fab fa-orcid text-info"></i>
                                                                </span>
                                                            </div>
                                                            <input type="text" class="form-control" name="addMP_NumPolice" placeholder="Entrer un n°" ng-model="FormAddMultirisquePro.numPolice" />
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6">
                                                        <label>Intermédiaire*</label>
                                                        <div class="input-group">
                                                            <div class="input-group-prepend">
                                                                <span class="input-group-text">
                                                                    <i class="fas fa-people-arrows text-info"></i>
                                                                </span>
                                                            </div>
                                                            <select class="form-control" name="addMP_Intermediaire" ng-model="FormAddMultirisquePro.intermediaire">
                                                                <option value="">Sélectionner un intermédiaire</option>
                                                                <option ng-repeat="lIntermediaire in listeIntermediaire" value="{{lIntermediaire.IT_CODEINTERMEDIAIRE}}">{{lIntermediaire.IT_DENOMMINATION}}</option>
                                                            </select>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="mb-1">
                                                <div class="form-group row">
                                                    <div class="col-lg-3">
                                                        <label>Pays*</label>
                                                        <div class="input-group">
                                                            <div class="input-group-prepend">
                                                                <span class="input-group-text">
                                                                    <i class="fas fa-globe-africa text-info"></i>
                                                                </span>
                                                            </div>
                                                            <select class="form-control" id="idPays" name="addMP_Pays" ng-model="FormAddMultirisquePro.pays" >
                                                                <option value="">Sélectionner un pays</option>
                                                                <option ng-repeat="lePays in listeDesPays" value="{{lePays.PY_CODEPAYS}}">{{lePays.PY_LIBELLE}}</option>
                                                            </select> <!--ng-selected="lePays.PY_CODEPAYS == '0001'"-->
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-3">
                                                        <label>Ville*</label>
                                                        <div class="input-group">
                                                            <div class="input-group-prepend">
                                                                <span class="input-group-text">
                                                                    <i class="fas fa-map-marker-alt text-info"></i>
                                                                </span>
                                                            </div>
                                                            <select class="form-control" id="idVille" name="addMP_Ville" ng-model="FormAddMultirisquePro.ville">
                                                                <option value="">Sélectionner une ville</option>
                                                                <option ng-repeat="laVille in listeDesVilles" value="{{laVille.VL_CODEVILLE}}">{{laVille.VL_LIBELLE}}</option>
                                                            </select>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-3">
                                                        <label>Commune*</label>
                                                        <div class="input-group">
                                                            <div class="input-group-prepend">
                                                                <span class="input-group-text">
                                                                    <i class="fas fa-map-pin text-info"></i>
                                                                </span>
                                                            </div>
                                                            <select class="form-control" name="addMP_Commune" ng-model="FormAddMultirisquePro.commune">
                                                                <option value="">Sélectionner une commune</option>
                                                                <option ng-repeat="laCommune in listeDesCommunes" value="{{laCommune.CO_CODECOMMUNE}}">{{laCommune.CO_LIBELLE}}</option>
                                                            </select>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-3">
                                                        <label>Situation géographique*</label>
                                                        <div class="input-group">
                                                            <div class="input-group-prepend">
                                                                <span class="input-group-text">
                                                                    <i class="fas fa-map text-info"></i>
                                                                </span>
                                                            </div>
                                                            <input type="text" class="form-control" name="addMP_SituationGeo" placeholder="Entrer une situation géograpgique" ng-model="FormAddMultirisquePro.situationGeo" />
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="mb-1">
                                                <div class="form-group row">
                                                    <div class="col-lg-3">
                                                        <label>Effet*</label>
                                                        <div class="input-group">
                                                            <div class="input-group-prepend">
                                                                <span class="input-group-text">
                                                                    <i class="far fa-calendar-alt text-info"></i>
                                                                </span>
                                                            </div>
                                                            <input type="text" class="form-control" name="addMP_Effet" placeholder="Entrer une date" ng-model="FormAddMultirisquePro.effet" />
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-4">
                                                        <label>Echéance*</label>
                                                        <div class="input-group">
                                                            <div class="input-group-prepend">
                                                                <span class="input-group-text">
                                                                    <i class="far fa-calendar-alt text-info"></i>
                                                                </span>
                                                            </div>
                                                            <input type="text" class="form-control" name="addMP_Echeance" placeholder="Entrer une date" ng-model="FormAddMultirisquePro.echeance" />
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-5">
                                                        <label>Assureur*</label>
                                                        <div class="input-group">
                                                            <div class="input-group-prepend">
                                                                <span class="input-group-text">
                                                                    <i class="fas fa-user-cog text-info"></i>
                                                                </span>
                                                            </div>
                                                            <select class="form-control" name="addMP_Assureur" ng-model="FormAddMultirisquePro.assureur">
                                                                <option value="">Sélectionner un assureur</option>
                                                                <option ng-repeat="lAssureur in listeDesAssureurs" value="{{lAssureur.TI_IDTIERS}}">{{lAssureur.TI_DENOMINATION}}</option>
                                                            </select>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="mb-1">
                                                <div class="form-group row">
                                                    <div class="col-lg-6">
                                                        <label>Interlocuteur*</label>
                                                        <div class="input-group">
                                                            <div class="input-group-prepend">
                                                                <span class="input-group-text">
                                                                    <i class="fas fa-assistive-listening-systems text-info"></i>
                                                                </span>
                                                            </div>
                                                            <input type="text" class="form-control" name="addMP_Interlocuteur" placeholder="Entrer un nom" ng-model="FormAddMultirisquePro.interlocuteur" />
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6">
                                                        <label>Téléphone interlocuteur*</label>
                                                        <div class="input-group">
                                                            <div class="input-group-prepend">
                                                                <span class="input-group-text">
                                                                    <i class="fas fa-phone-alt text-info"></i>
                                                                </span>
                                                            </div>
                                                            <input type="text" class="form-control" name="addMP_TelInterlocuteur" placeholder="Entrer un n° de téléphone" ng-model="FormAddMultirisquePro.telInterlocuteur" />
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="mb-1">
                                                <div class="form-group row">
                                                    <div class="col-lg-6">
                                                        <label>Désignation*</label>
                                                        <div class="input-group">
                                                            <div class="input-group-prepend">
                                                                <span class="input-group-text">
                                                                    <i class="fas fa-address-card text-info"></i>
                                                                </span>
                                                            </div>
                                                            <select class="form-control" name="addMP_Designation" ng-model="FormAddMultirisquePro.designation">
                                                                <option value="">Sélectionner une désignation</option>
                                                                <option ng-repeat="laDesignation in listeDesignation" value="{{laDesignation.DI_CODEDESIGNATION}}">{{laDesignation.DI_LIBELLEDESIGNATION}}</option>
                                                            </select>
                                                        </div>
                                                    </div>
                                                    <div class="col-lg-6">
                                                        <label>Type d'affaire*</label>
                                                        <div class="input-group">
                                                            <div class="input-group-prepend">
                                                                <span class="input-group-text">
                                                                    <i class="fas fa-briefcase text-info"></i>
                                                                </span>
                                                            </div>
                                                            <select class="form-control" name="addMP_TypeAffaire" ng-model="FormAddMultirisquePro.typeAffaire">
                                                                <option value="">Sélectionner un type</option>
                                                                <option ng-repeat="leTypeAffaire in listeTypeAffaire" value="{{leTypeAffaire.TA_CODETYPEAFFAIRES}}">{{leTypeAffaire.TA_LIBLLETYPEAFFAIRES}}</option>
                                                            </select>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="separator separator-dashed separator-success my-1"></div><br>
                                            <div class="mb-1">
                                                <div class="row">
                                                    <div class="col-12">
                                                        <table class="table table-bordered table-hover table-checkable" id="kt_datatable">
                                                            <!--tableau garenties-->
                                                            <thead style="text-align: center">
                                                                <tr>
                                                                    <th>LIBELLE SOUS CATEGORIE</th>
                                                                    <th>EVENEMENTS ASSURES</th>
                                                                    <th>CAPITAUX ASSURES</th>
                                                                    <th>PRIMES</th>
                                                                    <th>APRES REDUCTION</th>
                                                                    <th>PRORATA</th>
                                                                    <th>FRANCHISES</th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                <tr ng-repeat="garentie in tabGarenties">
                                                                    <td>{{garentie.SC_LIBELLESOUSCATEGORIE}}</td>
                                                                    <td>{{garentie.GA_LIBELLEGARANTIE}}</td>
                                                                    <td>
                                                                        <input type="text" name="" class="form-control" placeholder="" ng-model="garentie.CG_CAPITAUXASSURES" />
                                                                    </td>
                                                                    <td>
                                                                        <input type="text" name="" class="form-control" placeholder="" ng-model="garentie.CG_PRIMES" />
                                                                    </td>
                                                                    <td>
                                                                        <input type="text" name="" class="form-control" placeholder="" ng-model="garentie.CG_APRESREDUCTION" />
                                                                    </td>
                                                                    <td>
                                                                        <input type="text" name="" class="form-control" placeholder="" ng-model="garentie.CG_PRORATA" />
                                                                    </td>
                                                                    <td>
                                                                        <input type="text" name="" class="form-control" placeholder="" ng-model="garentie.CG_FRANCHISES" />
                                                                    </td>
                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="mb-1">
                                                <div class="row">
                                                    <div class="col-12">
                                                        <table class="table table-bordered table-hover table-checkable" id="kt_datatable">
                                                            <!--tableau primes-->
                                                            <thead style="text-align: center">
                                                                <tr>
                                                                    <th>LIBELLE</th>
                                                                    <th>PRIMES</th>
                                                                </tr>
                                                            </thead>
                                                            <tbody>
                                                                <tr ng-repeat="prime in tabPrimes">
                                                                    <td>{{prime.RE_LIBELLERESUME}}</td>
                                                                    <td>
                                                                        <input type="text" name="" class="form-control" placeholder="" ng-model="prime.CG_PRIMES" />
                                                                    </td>
                                                                </tr>
                                                            </tbody>
                                                        </table>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="card-footer">
                                <button type="submit" class="btn btn-primary btn-shadow-hover font-weight-bold" ng-click="ajoutFormAddMultirisquePro(FormAddMultirisquePro.effet, FormAddMultirisquePro.echeance)">
                                    <span>
                                        <i class="far fa-check-circle"></i>
                                        <span>Enregistrer</span>
                                    </span>
                                </button>&#160;&#160;
                                <button type="reset" class="btn btn-light-success font-weight-bold" ng-click="resetFormAddMultirisquePro()">
                                    <span>
                                        <i class="fas fa-undo"></i>
                                        <span>Réinitialiser</span>
                                    </span>
                                </button>
                                <a href="ListeMultirisqueProfessionnel" class="btn btn-icon btn-light-dark mr-2" data-toggle="tooltip" data-theme="dark" title="Retour" style="position: absolute; right: 27px">
                                    <i class="fas fa-arrow-left"></i>
                                </a>
                            </div>
                        </form>
                        <!--end::Form-->
                    </div>
                    <!--end::Card-->
                </div>
            </div>
        </div>
        <!--end::Container-->
    </div>
